function adh_debounce(func, wait, immediate) {
    var timeout;
    return function() {
        var context = this,
            args = arguments;
        var later = function() {
            timeout = null;
            if (!immediate) func.apply(context, args);
        };
        var callNow = immediate && !timeout;
        clearTimeout(timeout);
        timeout = setTimeout(later, wait);
        if (callNow) func.apply(context, args);
    };
}

function makeOmnivirt(id, width, height) {
    console.log('omnivirt ' + id);
    var o = makeOverlay();
    o.appendChild(addOmnivirtPlayer(id, width, height));
    console.log("player added to container")
        //o.appendChild(addCloseButton());
    document.body.appendChild(o);
    var videoFrameBox = document.getElementById("adh_frame_player");
    videoFrameBox.parentNode.insertBefore(addCloseButton(), videoFrameBox);
}

function makeYoutube(id, width, height) {
    console.log('youtube ' + id);
    var o = makeOverlay();
    o.appendChild(addYoutubePlayer(id, width, height));
    //o.appendChild(addCloseButton());
    document.body.appendChild(o);
    var videoFrameBox = document.getElementById("adh_frame_player");
    var videoFrameBox = document.getElementById("adh_frame_player");
    videoFrameBox.parentNode.insertBefore(addCloseButton(), videoFrameBox);
}

function makeOverlay() {
    var overlay = document.createElement('div');
    overlay.id = "overlay"
    overlay.style.width = "100%"; //document.body.clientWidth;
    overlay.style.height = "100%"; //document.body.clientHeight;
    overlay.style.backgroundColor = "rgba(237, 200, 161,0.5)";
    overlay.style.position = "fixed";
    overlay.style.top = 0;
    overlay.style.left = 0;
    overlay.style.margin = "10px;"
    overlay.style.zIndex = "998";
    return overlay;
}

function addOmnivirtPlayer(id, width, height) {
    var playerContainer = document.createElement('div');
    playerContainer.style.backgroundColor = "#33";
    playerContainer.style.height = "360px";
    playerContainer.style.width = "720px";
    playerContainer.style.margin = "0 auto";
    playerContainer.style.top = "25%";
    playerContainer.style.position = "relative";
    playerContainer.style.maxWidth = "100%";
    var innerHTML = '<script type="text\/javascript" src="\/\/upload.omnivirt.com/scripts/embed.js"><\/script>';
    innerHTML += '<div id="adh_frame_player"><iframe id="ado-24" src="https://www.vroptimal-3dx-assets.com/content/24?player=true" frameborder="0" width="' + width + '" height="' + height + '" webkitAllowFullScreen="1" mozallowfullscreen="1" allowFullScreen="1" style="border:1px white solid; box-shadow: 0px 0px 18px #888282;max-width:100%;"><\/iframe></div>';
    innerHTML += '<script type="text/javascript">document.getElementById("ado-' + id + '").setAttribute("src", "https://www.vroptimal-3dx-assets.com/content/' + id + '?player=true&referer=" + encodeURIComponent(window.location.href))<\/script> ';
    playerContainer.innerHTML = innerHTML;
    return playerContainer;
}

function addYoutubePlayer(id, width, height) {
    var playerContainer = document.createElement('div');
    playerContainer.style.backgroundColor = "#333";
    playerContainer.style.height = height + "px";
    playerContainer.style.width = width + "px";
    playerContainer.style.margin = "0 auto";
    playerContainer.style.top = "25%";
    playerContainer.style.position = "relative";
    playerContainer.style.maxWidth = "100%";
    var innerHTML = '<div id="adh_frame_player"><iframe width="' + width + '" height="' + height + '" src="https://www.youtube.com/embed/' + id + '?rel=0&autoplay=1&controls=2&modestbranding=1&fs=1" frameborder="0" allowfullscreen style="border:1px white solid; box-shadow: 0px 0px 18px #888282;max-width:100%;"></iframe></div>'
    playerContainer.innerHTML = innerHTML;
    return playerContainer;
}

function addCloseButton(params) {
    console.log('add Close button');
    var button = document.createElement("div");
    button.style.position = "absolute";
    button.style.right = "1px";
    button.style.top = "-31px";
    button.innerText = "X";
    button.style.color = "black";
    button.style.backgroundColor = "white";
    button.style.fontFamily = "Arial, sans";
    button.style.fontStyle = "bold";
    button.style.cursor = "pointer";
    button.style.fontSize = "20px";
    button.style.padding = "4px";
    button.style.paddingRight = "10px";
    button.style.paddingLeft = "10px";
    button.style.boxShadow = "0px 0px 1px #888282"
    button.addEventListener("click", cleanUp);
    return button;
}

function cleanUp(params) {
    var o = document.getElementById("overlay");
    o.style.display = "none";
    o.innerHTML = "";
    o.parentNode.removeChild(o);
    o.parentNode.removeChild(o);
}