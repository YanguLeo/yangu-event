function shuffle(array) {
    var currentIndex = array.length,
        temporaryValue, randomIndex;
    // While there remain elements to shuffle...
    while (0 !== currentIndex) {
        // Pick a remaining element...
        randomIndex = Math.floor(Math.random() * currentIndex);
        currentIndex -= 1;
        // And swap it with the current element.
        temporaryValue = array[currentIndex];
        array[currentIndex] = array[randomIndex];
        array[randomIndex] = temporaryValue;
    }
    return array;
}

function pickRandomElements(array, amount) {
    var result = [];
    while(amount > 0) {
        var randomIndex = Math.floor(Math.random() * array.length);
        result.push(array[randomIndex]);
        array.splice(randomIndex,1);
        amount--;
    }
    return result;
}

var updatePlayOverlay = adh_debounce(function() {
    var adImg = document.getElementsByClassName('c-adh-connect__block__image')[0]
    var overlayImg = document.getElementsByClassName('adh_play_overlay');
    var adOverlayDim = adImg.width / 6;

    for (var e = 0; e < overlayImg.length; e++) {
        overlayImg[e].style.width = adOverlayDim + 'px';
        overlayImg[e].style.left = ((adImg.width / 2) - adOverlayDim / 2) + 'px';
        overlayImg[e].style.top = ((adImg.height / 2) - adOverlayDim / 2) + 'px';
        overlayImg[e].style.display = 'block';
    }
}, 10);

