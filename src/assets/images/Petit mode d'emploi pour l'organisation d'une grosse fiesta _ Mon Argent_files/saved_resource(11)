  function shuffle(array) {
      var currentIndex = array.length,
          temporaryValue, randomIndex;
      // While there remain elements to shuffle...
      while (0 !== currentIndex) {
          // Pick a remaining element...
          randomIndex = Math.floor(Math.random() * currentIndex);
          currentIndex -= 1;
          // And swap it with the current element.
          temporaryValue = array[currentIndex];
          array[currentIndex] = array[randomIndex];
          array[randomIndex] = temporaryValue;
      }
      return array;
  }
  
  function divideAds(array) {
      for (x in array) {
          var item = array[x];
          if (item.type == 1) {
              partner_ads.push(item);
          } else if (item.type == 2) {
              shareholder_ads.push(item);
          } else if (item.type == 4) {
              talent_ads.push(item);
          }
      }
  }
  
  function fillArray(allAds, amount, chosenAds) {
      if (allAds.length > 0) {
          var orders = [],
              result = [],
              leftoverAds = [];
          if (chosenAds) result = chosenAds;
          for (x = 0; x < allAds.length; x++) {
              var item = allAds[x];
              if (orders.indexOf(item.order) == -1) {
                  result.push(item);
                  orders.push(item.order);
              } else {
                  leftoverAds.push(item)
              }
              if (result.length == amount) return result;
          }
          if (result.length < amount && leftoverAds.length > 0) {
              fillArray(leftoverAds, amount, result);
          }
          return result;
      }
  }
  
  function compileOutput(array, type) {
      var loc = document.domain;
      var thisOutput = '';
      var trackoutput = '';

      thisOutput += "<ul class='c-adh-partner-content__list'>";

      for (x = 0; x < array.length; x++) {
          var item = array[x];
          thisOutput += "<li class='c-adh-partner-content__list__item'> <a href='" + item.link + "' class='c-adh-partner-content__list__item__link'> <div class='c-adh-partner-content__image-wrapper'> <img class='lazyload' data-src='" + item.image + "' alt='…'> </div> <div class='c-adh-partner-content__content'> <span class='c-adh-partner-content__content__title'> " + item.titel + " </span> <span class='c-adh-partner-content__content__description'> " + item.tekst + " </span> </div> <span class='icon-arrow-right-nobg' aria-hidden='true'></span> </a> </li>";
          trackoutput += "<div style='position: absolute; left: 0px; top: 0px; visibility: hidden;'><img src='" + item.viewTrackUrl + "' border='0' width='1' height='1' alt='' style='display:none'></div>";
      }
      thisOutput += "</ul>";

      return thisOutput + trackoutput;
  }
  
