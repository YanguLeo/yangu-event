if (!String.prototype.includes) {
    String.prototype.includes = function(search, start) {
      'use strict';
      if (typeof start !== 'number') {
        start = 0;
      }
      if (start + search.length > this.length) {
        return false;
      } else {
        return this.indexOf(search, start) !== -1;
      }
    };
  }
  
  if (digitalData && digitalData.user && digitalData.user.segment && digitalData.user.segment.userType) {
    if(!adhese.request.ab || (adhese.request.ab && !adhese.request.ab.includes(digitalData.user.segment.userType))){
      adhese.registerRequestParameter("ab", digitalData.user.segment.userType);
    }   
  }
  
