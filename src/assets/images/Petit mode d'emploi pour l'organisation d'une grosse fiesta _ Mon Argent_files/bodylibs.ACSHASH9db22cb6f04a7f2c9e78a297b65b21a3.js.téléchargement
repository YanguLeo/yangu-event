}

ï»¿"use strict"

//
// Omrekening rentevoeten ---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//

function p2y(rate, frequency, type)
{
    var ret = 0;

    if (type == 0)          // rentevoet / aantal periodes per jaar
    {
        ret = (rate / 100) * frequency;
    }
    else if (type == 1)     // actuarieel
    {
        ret = Math.pow(1 + (rate / 100), frequency) - 1;
    }
    return (ret * 100);
}

//

function y2p(rate, frequency, type)
{
    var ret = 0;

    if (type == 0 && frequency != 0)            // rentevoet / aantal periodes per jaar
    {
        ret = (rate / 100) / frequency;
    }
    else if (type == 1 && frequency != 0)       // actuarieel
    {
        ret = Math.pow(1 + (rate / 100), 1 / frequency) - 1;
    }
    else
    {
        ret = 0;
    }
    return (ret * 100);
}

//
// Bereken FV ---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//

function fv(pv, pmt, nper, rate, numerando, frequency, type)
{
    // fv           toekomstige waarde van een geldsom
    // pv           huidig kapitaal
    // pmt          periodieke stortingen
    // nper         aantal stortingen + looptijd in periodieke eenheden
    // rate         rentevoet 
    // numerando    tijdstip van de stortingen 0=begin periode 1=einde periode
    // frequency    aantal stortingen per jaar
    // type         rentevoet actuarieel berekenen of niet 0=nee 1=ja

    var ret = { fv: 0 };

    var rente = y2p(rate, frequency, type);

    var v1 = 0;
    var v2 = 0;

    //

    rente = rente / 100;

    v1 = pv * Math.pow(1 + rente, nper);

    if (numerando === 0)
    {
        v2 = pmt * ((Math.pow(1 + rente, nper) - 1) / rente) * (1 + rente);
    }
    else if (numerando === 1)
    {
        v2 = pmt * ((Math.pow(1 + rente, nper) - 1) / rente);
    }

    ret.fv = v1 + v2;

    return (ret);
}

//
// Bereken PV ---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//

function pv(fv, pmt, nper, rate, numerando, frequency, type)
{
    // pv           huidige waarde van een geldsom
    // fv           toekomstig kapitaal
    // pmt          periodieke stortingen
    // nper         aantal stortingen + looptijd in periodieke eenheden
    // rate         rentevoet 
    // numerando    tijdstip van de stortingen 0=begin periode 1=einde periode
    // frequency    aantal stortingen per jaar
    // type         rentevoet actuarieel berekenen of niet 0=nee 1=ja

    var ret = { pv: 0 };

    var rente = y2p(rate, frequency, type);

    var v1 = 0;
    var v2 = 0;

    //

    rente = rente / 100;

    if (pmt != 0 && fv == 0)
    {
        if (numerando == 0)
        {
            ret.pv = pmt * ((1 - Math.pow(1 + rente, -nper)) / rente) * (1 + rente);
        }
        else if (numerando == 1)
        {
            ret.pv = pmt * ((1 - Math.pow(1 + rente, -nper)) / rente);
        }
    }
    else if (pmt == 0 && fv != 0)
    {
        ret.pv = fv / Math.pow(1 + rente, nper);
    }
    else if (pmt != 0 && fv != 0)
    {
        if (numerando == 0)
        {
            v1 = ((pmt * (Math.pow(1 + rente, nper) - 1)) / rente) * (1 + rente);
        }
        else if (numerando == 1)
        {
            v1 = (pmt * (Math.pow(1 + rente, nper) - 1)) / rente;
        }

        v2 = fv - v1;
        ret.pv = v2 / Math.pow(1 + rente, nper);
    }

    return (ret);
}

//
// Bereken PV ---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//

function pmt(pv, fv, nper, rate, numerando, frequency, type)
{
    // pv           huidge kapitaal
    // fv           gewenst toekomstig kapitaal
    // nper         aantal stortingen + looptijd in periodieke eenheden
    // rate         rentevoet 
    // numerando    tijdstip van de stortingen 0=begin periode 1=einde periode
    // frequency    aantal stortingen per jaar
    // type         rentevoet actuarieel berekenen of niet 0=nee 1=ja

    var ret = { pmt: 0 };

    var rente = y2p(rate, frequency, type);

    var v1 = 0;
    var v2 = 0;

    //

    rente = rente / 100;

    if (pv != 0 && fv == 0)
    {
        if (numerando == 0)
        {
            ret.pmt = pv * (rente / (1 - Math.pow(1 + rente, -nper)));
        }
        else if (numerando == 1)
        {
            ret.pmt = (pv * (rente / (1 - Math.pow(1 + rente, -nper)))) * (1 / (1 + rente));
        }
    }
    else if (pv == 0 && fv != 0)
    {
        if (numerando == 0)
        {
            ret.pmt = fv / ((Math.pow(1 + rente, nper) - 1) / rente);
        }
        else if (numerando == 1)
        {
            ret.pmt = fv / ((Math.pow(1 + rente, nper) - 1) / rente) * (1 / (1 + rente));
        }
    }
    else if (pv != 0 && fv != 0)
    {
        v1 = pv * Math.pow(1 + rente, nper);
        v2 = fv - v1;

        if (numerando == 0)
        {
            ret.pmt = v2 / ((Math.pow(1 + rente, nper) - 1) / rente)
        }
        else if (numerando == 1)
        {
            ret.pmt = v2 / ((Math.pow(1 + rente, nper) - 1) / rente) * (1 / (1 + rente));
        }
    }

    return (ret);
}

//
// Bereken NPER TO FV -------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//

function nper2save(pv, pmt, objective, rate, numerando, frequency, type)
{
    // pv           huidig kapitaal
    // pmt          periodieke betaling
    // objective    gewenst toekomstig kapitaal
    // rate         rentevoet 
    // numerando    tijdstip van de stortingen 0=begin periode 1=einde periode
    // frequency    aantal stortingen per jaar
    // type         rentevoet actuarieel berekenen of niet 0=nee 1=ja

    var ret = { nper: 0, nper2use: 0, year: 0, month: 0 };

    var rente = y2p(rate, frequency, type);

    var teller = 0;
    var noemer = 0;

    var nper = 0;
    var nper2use = 0;

    var year = 0;
    var month = 0;

    var remainder = 0;

    var min = 0;
    var max = 0;
    var guess = 0;

    var v = 0;

    var w;

    var t = "";

    //

    rente = rente / 100;

    if (pv != 0 && pmt == 0)
    {
        teller = Math.log(objective / pv);
        noemer = Math.log(1 + rente);
        nper = teller / noemer;
    }
    else if (pv == 0 && pmt != 0)
    {
        if (numerando == 0)
        {
            teller = Math.log(objective * rente + pmt * (1 + rente)) - Math.log(pmt * (1 + rente));
            noemer = Math.log(1 + rente);
            nper = teller / noemer;
        }
        else if (numerando == 1)
        {
            teller = Math.log(objective * rente + pmt) - Math.log(pmt);
            noemer = Math.log(1 + rente);
            nper = teller / noemer;
        }
    }
    else if (pv != 0 && pmt != 0)
    {
        guess = (objective - pv) / pmt;

        min = 0;
        max = guess;

        w = fv(pv, pmt, guess, rate, numerando, frequency, type);
        
        while (v < 40)
        {
            if (w.fv > objective)
            {
                guess = guess - (max - min) / 2;
                min = guess;
            }
            else if (w.fv < objective)
            {
                guess = guess + (max - min) / 2;
                max = guess;
            }

            w = fv(pv, pmt, guess, rate, numerando, frequency, type);
            v++;

            t += min + " | " + max + " | " + guess + " | " + w.fv + "\n";
        }

        nper = guess;
    }

    nper2use = int(nper) + 1;

    ret.nper = nper;
    ret.nper2use = nper2use;

    // Omzetten naar jaren en maanden

    if (frequency == 12)
    {
        year = int(nper2use / frequency);
        remainder = nper2use - (year * frequency);
        month = remainder;
    }
    else if (frequency == 1)
    {
        year = nper2use;
        month = 0;
    }
    else 
    {
        year = int(nper2use / frequency);
        remainder = nper2use - (year * frequency);
        month = remainder * (12 / frequency);
    }

    ret.year = year;
    ret.month = month;

    // 

    return (ret);
}

//
// Bereken NPER TO FV -------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//

function nper2spend(pv, pmt, rate, numerando, frequency, type, keep)
{
    // pv           huidig kapitaal
    // pmt          periodieke opname
    // rate         rentevoet 
    // numerando    tijdstip van de opname 0=begin periode 1=einde periode
    // frequency    aantal opnames per jaar
    // type         rentevoet actuarieel berekenen of niet 0=nee 1=ja
    // keep         kapitaal te behouden

    var ret = { nper: 0, nper2use: 0, year: 0, month: 0, pmt: 0 };

    var fv = 0;
    var pmt2use = 0;

    var nper = 0;
    var nper2use = 0;

    var rente = y2p(rate, frequency, type);

    var year = 0;
    var month = 0;

    var remainder = 0;

    //

    rente = rente / 100;

    //

    if (keep <= 100)
    {
        fv = pv * (keep / 100);
    }
    else
    {
        fv = (keep > pv ? pv : fv);
    }

    //

    if (keep == 100)
    {
        pmt2use = pv * rente;
        nper = 999;
    }
    else 
    {
        pmt2use = pmt * -1;

        if (numerando == 0)
        {
            nper = Math.log((fv + (pmt2use * (1 + rente)) / rente) / (pv + (pmt2use * (1 + rente)) / rente)) / Math.log(1 + rente);
        }
        else if (numerando == 1)
        {
            nper = Math.log((fv + pmt2use / rente) / (pv + pmt2use / rente)) / Math.log(1 + rente);
        }
    }

    //

    nper2use = int(nper);

    ret.nper = nper;
    ret.nper2use = nper2use;
    ret.pmt = pmt2use;

    // Omzetten naar jaren en maanden

    if (frequency == 12)
    {
        year = int(nper2use / frequency);
        remainder = nper2use - (year * frequency);
        month = remainder;
    }
    else if (frequency == 1)
    {
        year = nper2use;
        month = 0;
    }
    else 
    {
        year = int(nper2use / frequency);
        remainder = nper2use - (year * frequency);
        month = remainder * (12 / frequency);
    }

    ret.year = year;
    ret.month = month;

    // 

    return (ret);
}

//
// Bereken NPV --------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//

function npv(flow, rate)
{
    var npv = 0
    var n = 0
    var i = 0

    n = flow.length - 1

    for (i = 0; i <= n; i++)
    {
        npv = npv + (flow[i] / Math.pow(1 + rate, i))
    }
    return (npv)
}

//
// Bereken IRR --------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//

function irr(flow)
{
    var under = 0
    var upper = 1
    var guess = 0.5

    var i = 1
    var n = 0

    //

    while (i != 0 && n <= 50)
    {
        i = npv(flow, guess)

        if (i > 0) 
        {
            under = guess
            guess = guess + ((upper - guess) / 2)
        }
        else if (i < 0)
        {
            upper = guess
            guess = guess - ((guess - under) / 2)
        }

        n++
    }

    return (guess * 100)
}

//
// Bereken Spaarrekening ----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//

function savingsAccount(monthBegin, pv, pmt, per, frek, rate)
{
    var R1 = 0;
    var R2 = 0;

    var wPmtYear = 0;
    var wSumPv = 0;

    var wMonthEnd = 0;

    var wBeginPeriode = 0;
    var wEindPeriode = 0;

    // Normaal schema of gebroken beginjaar en eindjaar ...

    if (wMonthBegin == 1) 
    {
        R1 = savingsAccountRente(1, 12, pmt, frek, rate);
        wPmtYear = savingsAccountPmt(1, 12, pmt, frek);
        wSumPv = wPv;

        for (i = 1; i <= per; i++) 
        {
            R2 = wSumPv * (rate / 100);
            wSumPv = wSumPv + wPmtYear + R1 + R2
        }
    }
    else if (wMonthBegin != 1) 
    {
        wBeginPeriode = 12 - monthBegin + 1;
        wEindPeriode = 12 - (12 - monthBegin) - 1;
        wMonthEnd = 12 - (12 - monthBegin) - 1;

        // Eerste periode 

        R1 = savingsAccountRente(monthBegin, 12, pmt, frek, rate);
        wPmtYear = savingsAccountPmt(monthBegin, 12, pmt, frek);
        wSumPv = wPv;
        R2 = wSumPv * (rate / 100) * (wBeginPeriode / 12);
        wSumPv = wSumPv + wPmtYear + R1 + R2

        // Normale periode 

        R1 = savingsAccountRente(1, 12, pmt, frek, rate);
        pmtYear = savingsAccountPmt(1, 12, pmt, frek);

        for (i = 2; i <= per; i++)
        {
            R2 = wSumPv * (rate / 100);
            wSumPv = wSumPv + wPmtYear + R1 + R2
        }

        // Laatste periode 

        R1 = savingsAccountRente(1, wEindPeriode, pmt, frek, rate);
        wPmtYear = savingsAccountPmt(1, wMonthEnd, pmt, frek);
        R2 = wSumPv * (rate / 100) * (wEindPeriode / 12);
        wSumPv = wSumPv + wPmtYear + R1 + R2
    }

    return (wSumPv)
}

//

function berekenRente(monthBegin, monthEnd, pmt, frequency, rate)
{
    var i = 0;
    var rente = 0;
    var frek = 12 / frequency

    for (i = monthBegin; i <= monthEnd; i = i + frek)
    {
        if (i == 1)
        {
            rente = rente + (pmt * (rate / 100) * (365 / 365))
        }
        else if (i == 2)
        {
            rente = rente + (pmt * (rate / 100) * (334 / 365))
        }
        else if (i == 3)
        {
            rente = rente + (pmt * (rate / 100) * (306 / 365))
        }
        else if (i == 4)
        {
            rente = rente + (pmt * (rate / 100) * (275 / 365))
        }
        else if (i == 5)
        {
            rente = rente + (pmt * (rate / 100) * (245 / 365))
        }
        else if (i == 6)
        {
            rente = rente + (pmt * (rate / 100) * (214 / 365))
        }
        else if (i == 7)
        {
            rente = rente + (pmt * (rate / 100) * (184 / 365))
        }
        else if (i == 8)
        {
            rente = rente + (pmt * (rate / 100) * (153 / 365))
        }
        else if (i == 9)
        {
            rente = rente + (pmt * (rate / 100) * (122 / 365))
        }
        else if (i == 10)
        {
            rente = rente + (pmt * (rate / 100) * (92 / 365))
        }
        else if (i == 11)
        {
            rente = rente + (pmt * (rate / 100) * (61 / 365))
        }
        else if (i == 12)
        {
            rente = rente + (pmt * (rate / 100) * (31 / 365))
        }
    }

    return (rente);
}

function savingsAccountPmt(monthBegin, monthEnd, pmt, frequency)
{
    var i = 0;
    var pv = 0;
    var frek = 12 / frequency;

    for (i = monthBegin; i <= monthEnd; i = i + frek)
    {
        pv += pmt;
    }

    return (pv)
}  
ï»¿// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  BEREKEN BELASTING OP HET MARGINALE GEDEELTE EXTRA INKOMEN
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenTaxExtraVerdienen(inkomenBasis, inkomenExtra, gemTax)
{
    var ret = { taxBasis: 0, taxTotaal: 0, taxMarginaal: 0, netto: 0 };

    var taxBasis = berekenPB(inkomenBasis);
    taxBasis = taxBasis + (taxBasis * (gemTax / 100));

    var taxTotaal = berekenPB(inkomenBasis + inkomenExtra);
    taxTotaal = taxTotaal + (taxTotaal * (gemTax / 100));

    var taxMarginaal = taxTotaal - taxBasis;

    ret.taxBasis = taxBasis;
    ret.taxTotaal = taxTotaal;
    ret.taxMarginaal = taxMarginaal;
    ret.netto = inkomenExtra - taxMarginaal;

    return (ret)
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  BEREKEN BELASTING OP HET MARGINALE GEDEELTE EXTRA INKOMEN
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenTax(inkomen, gemTax)
{
    var tax = 0;

    tax = berekenPB(inkomen);
    tax = tax + (tax * (gemTax / 100));

    return (tax)
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  BEREKEN PB
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenPB(inkomen)
{
    var schijven = new Array(0, 13250, 23390, 40480);
    var tarieven = new Array(25, 40, 45, 50);

    var ret = cTax(inkomen, schijven, tarieven);

    return (ret);
}

function cTax(b, s, t)
{
    var i = 0;
    var r = 0;
    var u = 0;

    u = s.length - 1

    for (i = 1; i <= u; i++)
    {
        if (b > s[i - 1] && b <= s[i])
        {
            r = r + ((b - s[i - 1]) * (t[i - 1] / 100))
        }
        else if (b > s[i])
        {
            r = r + ((s[i] - s[i - 1]) * (t[i - 1] / 100))
        }
    }

    // Correctie indien groter dan laatste

    if (b > s[u])
    {
        r = r + ((b - s[u]) * (t[u] / 100))
    }

    return roundit(r, 2)
}

ï»¿//----------------------------------------------------------------------------------------------------------------------------------------
// AFDRUK: TEKST + ARRAY + OBJECT
//----------------------------------------------------------------------------------------------------------------------------------------

function doPrint(w)
{
    nw = window.open("", "", "toolbar=yes,status=no,menubar=no,scrollbars=yes,alwaysraised=yes,resizable=yes,width=760,height=780")
    nw.document.write(w);
    nw.document.close();
}

function printTabel(o, age)
{
    var i = 0;
    var t = "";

    t = "<table style='font-family: Tahoma'>"

    for (i = 0; i <= o.length - 1; i++)
    {
        t += "<tr>"
        t += "<td>" + i + "</td>"
        t += "<td>" + (i + 1) + "</td>"
        t += "<td>" + (age + i) + "</td>"
        t += "<td>" + o[i] + "</td>"
        t += "</tr>"
    }

    t += "</table>"

    doPrint(t);
}

function printObject(o)
{
    doPrint(JSON.stringify(o, null, 2))
}



ï»¿"use strict"

//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// PREMIE AO
//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenPremieAO(wInvaliditeit, wBeroep, wLeeftijd, wEindLeeftijd, wWaarborg, wJaarPremie, wJaarRente, wPremieBetaling, wTypeRente, wFormule, wERT, wERT60, wBijpremieMedisch, wBijpremieAndere, wKlasse, wProfiel, wBachelor, wPersoneel, wManueel, wCommissie)
{
    var beginKolom = 0;
    var kolomIndex = 0;

    var ERT60ToUse = 0;

    var basisTarief = 0;

    var ret = { klasse: 0, C1: 0, C2: 0, C2_rente: 0, GI: 0, GI_rente: 0 }

    // Bereken basistarief

    if (wEindLeeftijd <= 55)
    {
        beginKolom = 53
    }
    else if (wEindLeeftijd >55 && wEindLeeftijd <= 60)
    {
        beginKolom = 35
    }
    else if (wEindLeeftijd > 60 && wEindLeeftijd <= 65)
    {
        beginKolom = 2
    }

    var a = (beginKolom == 2 ? 11 : 6)
    var b = (beginKolom == 2 ? 2 : 1)

    var xLeeftijd = (wLeeftijd < 20 ? 20 : wLeeftijd);

    // Check wERT60

    ERT60ToUse = (wERT60 == 0 && beginKolom == 2 && wERT != 6 ? 1 : 0)

    // Bereken kolomindex en basistarief

    kolomIndex = beginKolom + ((wTypeRente - 1) * a) + ((wERT - 1) * b) + ERT60ToUse;
    basisTarief = parseFloat(tarief[xLeeftijd - 20][kolomIndex - 1] / 1000);

    // Bereken klasse

    var klasseToUse = berekenKlasse(wKlasse, wProfiel, wBachelor, wPersoneel, wManueel);

    // Klasse

    var coefKlasse = berekenCoefficientKlasse(klasseToUse);

    // Invaliditeit

    var coefInvaliditeit = berekenCoefficientInvaliditeit(wInvaliditeit);

    // Renteklasse

    var coefRente = berekenCoefficientRente(wWaarborg, wJaarRente);

    // Formule

    var coefFormule = berekenCoefficientFormule(wFormule);

    // Beheer

    var coefBeheer = berekenCoefficientBeheer(wWaarborg);

    // Periodiciteit

    var coefPeriodiciteit = berekenCoefficientPeriodiciteit(wPremieBetaling);

    // Bijpremie

    var coefBijPremie =  1 + ((wBijpremieMedisch + wBijpremieAndere) / 100);

    // Commissie

    var coefCommissie = wCommissie / 100;

    // Taksen

    var coefTax = 0.0925;

    // Bereken premies

    var C1 = 0;
    var C2 = 0;
    var GI = 0;

    var C2_rente = 0;
    var GI_rente = 0;

    //
    //alert((basisTarief + "\n" + wJaarPremie + "\n" + coefKlasse + "\n" + coefInvaliditeit + "\n" + coefRente + "\n" + coefFormule + "\n" + coefBeheer + "\n" + coefPeriodiciteit + "\n" + coefBijPremie) + "\n" + (1 - coefCommissie))
    //

    if (wWaarborg == 1)
    {
        C1 = (basisTarief * wJaarPremie * coefKlasse * coefInvaliditeit * coefRente * coefFormule * coefBeheer * coefPeriodiciteit * coefBijPremie) / (1 - coefCommissie);
    }
    else if (wWaarborg == 2)
    {
        C1 = (basisTarief * wJaarPremie * coefKlasse * coefInvaliditeit * coefRente * coefFormule * coefBeheer * coefPeriodiciteit * coefBijPremie) / (1 - coefCommissie);

        //

        C2 = (basisTarief * wJaarRente * coefKlasse * coefInvaliditeit * coefRente * coefFormule * coefBeheer * coefPeriodiciteit * coefBijPremie) / (1 - coefCommissie);

        if (C2 > wJaarPremie - C1)
        {
            C2 = wJaarPremie - C1;
        }

        C2_rente = (C2 * (1 - coefCommissie)) / (coefBijPremie * coefPeriodiciteit * coefBeheer * coefFormule * coefRente * coefInvaliditeit * coefKlasse * basisTarief); 
        C2 = C2 * (1 + coefTax);

        //

        coefBeheer = berekenCoefficientBeheer(3);

        GI_rente = (C2 != wJaarRente ? wJaarRente - C2_rente : 0);
        GI = (basisTarief * GI_rente * coefKlasse * coefInvaliditeit * coefRente * coefFormule * coefBeheer * coefPeriodiciteit * coefBijPremie) / (1 - coefCommissie);
        GI = GI * (1 + coefTax);

    }
    else if (wWaarborg == 3)
    {
        GI_rente = wJaarRente;
        GI = (basisTarief * GI_rente * coefKlasse * coefInvaliditeit * coefRente * coefFormule * coefBeheer * coefPeriodiciteit * coefBijPremie) / (1 - coefCommissie);
        GI = GI * (1 + coefTax);

        C1 = 0;
        C2 = 0;
    }

    ret.klasse = klasseToUse;

    ret.C1 = C1;

    ret.C2 = C2;
    ret.C2_rente = C2_rente;

    ret.GI = GI;
    ret.GI_rente = GI_rente;

    return (ret);
}

//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenKlasse(basis, profiel, bachelor, personeel, manueel)
{
    var klasse = basis;
    var correctie = 0;

    var correctieBachelor = 0;
    var correctiePersoneel = 0;
    var correctieManueel = 0;

    var minimum = 0;

    //

    if (profiel == "a")
    {
        correctieBachelor = 0;
        correctiePersoneel = 1;
        correctieManueel = 1;

        minimum = 1;
    }
    else if (profiel == "b")
    {
        correctieBachelor = (bachelor == 1 ? -2 : 0);
        correctiePersoneel = (personeel > 6 ? -1 : 1);
        correctieManueel = 1;

        minimum = 2;
    }
    else if (profiel == "g")
    {
        correctieBachelor = (bachelor == 1 ? -1 : 0);
        correctiePersoneel = 1;
        correctieManueel = (manueel == 1 ? 1 : -2);

        minimum = 1;
    }

    correctie = correctieBachelor + correctiePersoneel + correctieManueel;
    klasse = Math.max(klasse + correctie, minimum);

    return (klasse);
}

function berekenCoefficientKlasse(klasse)
{
    var coef = [0, 1, 1.2, 1.5, 2.3, 3.3];
    return (coef[klasse])        
}

function berekenCoefficientInvaliditeit(type)
{
    var coef = [0, 1, 1.12];
    return (coef[type])
}

function berekenCoefficientRente(waarborg, rente)
{
    var retval = 0;

    if (waarborg == 1)
    {
        retval = 1;
    }
    else
    {
        if (rente < 15000)
        {
            retval = 1;
        }
        else if (rente >= 15000 && rente < 30000)
        {
            retval = 0.85;
        }
        else if (rente >= 30000)
        {
            retval = 0.70;
        }
    }

    return (retval)
}

function berekenCoefficientFormule(formule)
{
    var coef = [0, 1, 0.9, 0.8];
    return (coef[formule])        
}

function berekenCoefficientBeheer(waarborg)
{
    var coef = [0, 1.25, 1.25, 1.75];
    return (coef[waarborg])
}

function berekenCoefficientPeriodiciteit(periodiciteit)
{
    var coef = [0, 1.04, 1.03, 1.02, 1.00];
    return (coef[periodiciteit])
}

//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// 80% regels
//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function bereken80_consumer(wBrutoLoon, wBrutoUitkering)
{
    var ret = { maxBruto: 0, maxNetto: 0 };

    var maxBruto = 0.80 * wBrutoLoon - wBrutoUitkering; 

    ret.maxBruto = maxBruto;
    return (ret);
}

function bereken80_broker_wn(wBrutoLoon, wBrutoUitkering, wDertiendeMaand, wVakantiegeld, wRenteAO, wMaxUitkeringRiziv)
{
    var ret = { maxBruto: 0 };

    var coefficientInkomen = 0;
    var brutoJaarinkomen = 0;
    var rizivPlafond = 0;
    var rizivExcedent = 0;
    var maxBruto = 0;

    //

    var v = (wVakantiegeld == 1 ? 0.92 : 0)
    var d = (wDertiendeMaand == 1 ? 1 : 0)

    coefficientInkomen = 12 + v + d;

    //

    brutoJaarinkomen = wBrutoLoon * coefficientInkomen;
    rizivPlafond = wMaxUitkeringRiziv * 312;

    rizivExcedent = brutoJaarinkomen - rizivPlafond;
    rizivExcedent = (rizivExcedent < 0 ? 0 : rizivExcedent);

    maxBruto = roundit(((0.20 * rizivPlafond) + (0.80 * rizivExcedent) - wRenteAO), 2);
    maxBruto = roundit(maxBruto / 12, 2);
    maxBruto = (maxBruto < 0 ? 0 : maxBruto);

    ret.maxBruto = maxBruto;

    return (ret);
}

function bereken80_broker_zelfstandige(wBrutoLoon, wRenteAO)
{
    var ret = { maxBruto: 0 };
    var maxBruto = 0;

    //

    maxBruto = roundit(0.80 * wBrutoLoon - wRenteAO, 2);
    maxBruto = (maxBruto < 0 ? 0 : maxBruto);

    ret.maxBruto = maxBruto;

    return (ret);
}
