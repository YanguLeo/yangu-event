                    else if (o[i].zorg == 3)
                    {
                        zorgtoeslag += 431.00
                    }
                    else if (o[i].zorg == 4)
                    {
                        zorgtoeslag += 261.10
                    }
                    else if (o[i].zorg == 5)
                    {
                        zorgtoeslag += 431.00
                    }
                    else if (o[i].zorg == 6)
                    {
                        zorgtoeslag += 431.00
                    }
                    else if (o[i].zorg == 7)
                    {
                        zorgtoeslag += 490.07
                    }
                    else if (o[i].zorg == 8)
                    {
                        zorgtoeslag += 525.08
                    }
                    else if (o[i].zorg == 9)
                    {
                        zorgtoeslag += 560.08
                    }
                }

                // Sociale toeslag

                socialetoeslag = 0;

                // Toeslag eenoudergezin

                if (o[i].eenoudergezin == 1 && i == 0)
                {
                    eenoudertoeslag = 48.77;
                }
                else if (o[i].eenoudergezin == 1 && i == 1)
                {
                    eenoudertoeslag = 30.23;
                }
                else if (o[i].eenoudergezin == 1 && i >= 2)
                {
                    eenoudertoeslag = 24.38;
                }

                // Kindergeld 

                k = basis + leeftijdstoeslag + zorgtoeslag + socialetoeslag + eenoudertoeslag;

                kindergeld += k;
                detail.push({ geboortejaar: geboortejaar, kindergeld: k });
            }
        }
    }
    else if (gewest == 2)
    {
        for (i = 0; i <= o.length - 1; i++)
        {
            basis = 0;
            leeftijdstoeslag = 0;
            wezentoeslag = 0;
            pleegzorgtoeslag = 0;
            zorgtoeslag = 0;
            socialetoeslag = 0;
            schoolbonus = 0;
            kinderopvangtoeslag = 0;
            kleutertoeslag = 0;
            eenoudertoeslag = 0;
            schooltoeslag = 0;
            geboortepremie = 0;

            //

            geboortejaar = jaar - o[i].leeftijd;

            //

            if (geboortejaar >= 2020)
            {
                // geboortepremie eenmalig

                geboortepremie = 1100;

                //

                if (o[i].weeskind == 1)
                {
                    basis = 350;
                }
                else
                {
                    if (o[i].leeftijd >= 0 && o[i].leeftijd < 18)
                    {
                        basis = 155
                    }
                    else if (o[i].leeftijd >= 18 && o[i].leeftijd < 25)
                    {
                        basis = 165
                    }

                }

                // Sociale toeslagen

                if (o[i].inkomen <= 30000)
                {
                    socialetoeslag += 55;

                    if (o[i].ouderinvalide == 1)
                    {
                        socialetoeslag += 10;
                    }

                    if (o[i].eenoudergezin == 1)
                    {
                        socialetoeslag += 20;
                    }

                    if (o.length >= 3)
                    {
                        socialetoeslag += 35;
                    }
                }
                if (o[i].inkomen > 30000 && inkomen < 50000)
                {
                    socialetoeslag += 25;

                    if (o[i].ouderinvalide == 1)
                    {
                        socialetoeslag += 0;
                    }

                    if (o[i].eenoudergezin == 1)
                    {
                        socialetoeslag += 10;
                    }

                    if (o.length >= 3)
                    {
                        socialetoeslag += 20;
                    }
                }

                // Wezentoeslag 

                if (o[i].halfweeskind == 1)
                {
                    if (o[i].leeftijd >= 0 &&  o[i].leeftijd <= 17)
                    {
                        wezentoeslag = 77;
                    }
                    else if (o[i].leeftijd >= 18 && o[i].leeftijd <= 24)
                    {
                        wezentoeslag = 82;
                    }
                }

                // Zorgtoeslag

                if (o[i].leeftijd < 21)
                {
                    if (o[i].zorg == 1)
                    {
                        zorgtoeslag += 84.01
                    }
                    else if (o[i].zorg == 2)
                    {
                        zorgtoeslag += 111.89
                    }
                    else if (o[i].zorg == 3)
                    {
                        zorgtoeslag += 431.00
                    }
                    else if (o[i].zorg == 4)
                    {
                        zorgtoeslag += 261.10
                    }
                    else if (o[i].zorg == 5)
                    {
                        zorgtoeslag += 431.00
                    }
                    else if (o[i].zorg == 6)
                    {
                        zorgtoeslag += 431.00
                    }
                    else if (o[i].zorg == 7)
                    {
                        zorgtoeslag += 490.07
                    }
                    else if (o[i].zorg == 8)
                    {
                        zorgtoeslag += 525.08
                    }
                    else if (o[i].zorg == 9)
                    {
                        zorgtoeslag += 560.08
                    }
                }

                // Schooltoeslag

                if (o[i].leeftijd >= 0 && o[i].leeftijd <= 5)
                {
                    schooltoeslag = 20;
                }
                else if (o[i].leeftijd >= 6 && o[i].leeftijd <= 11)
                {
                    schooltoeslag = 30;
                }
                else if (o[i].leeftijd >= 12 && o[i].leeftijd <= 17)
                {
                    schooltoeslag = 50;
                }
                else if (o[i].leeftijd >= 18 && o[i].leeftijd <= 24)
                {
                    schooltoeslag = 80;
                }

                // kindergeld 

                k = basis + socialetoeslag + wezentoeslag + zorgtoeslag;

                kindergeld += k;
                detail.push({ geboortejaar: geboortejaar, kindergeld: k, geboortepremie: geboortepremie, schooltoeslag: schooltoeslag });

            }
            else if (geboortejaar < 2020)
            {
                if (o[i].weeskind == 1 || o[i].halfweeskind == 1)
                {
                    basis = 368.03;
                }
                else if (o[i].pleegkind == 1)
                {
                    basis = 64.28;
                }
                else
                {
                    if (i == 0)
                    {
                        basis = 95.80;
                    }
                    else if (i == 1)
                    {
                        basis = 177.77;
                    }
                    else if (i >= 2)
                    {
                        basis = 264.67;
                    }
                }

                // Leeftijdstoeslag

                if (i == 0)
                {
                    if (o[i].leeftijd >= 6 && o[i].leeftijd <= 11)
                    {
                        leeftijdstoeslag = 16.69;
                    }
                    else if (o[i].leeftijd >= 12 && o[i].leeftijd <= 17)
                    {
                        leeftijdstoeslag = 25.41;
                    }
                    else if (o[i].leeftijd >= 18 && o[i].leeftijd <= 24)
                    {
                        leeftijdstoeslag = 29.29;
                    }
                }
                else if (i > 0)
                {
                    if (o[i].leeftijd >= 6 && o[i].leeftijd <= 11)
                    {
                        leeftijdstoeslag = 33.28;
                    }
                    else if (o[i].leeftijd >= 12 && o[i].leeftijd <= 17)
                    {
                        leeftijdstoeslag = 50.86;
                    }
                    else if (o[i].leeftijd >= 18 && o[i].leeftijd <= 24)
                    {
                        leeftijdstoeslag = 64.66;
                    }
                }

                // Zorgtoeslag 

                if (o[i].leeftijd < 21)
                {
                    if (o[i].zorg == 1)
                    {
                        zorgtoeslag += 84.01
                    }
                    else if (o[i].zorg == 2)
                    {
                        zorgtoeslag += 111.89
                    }
                    else if (o[i].zorg == 3)
                    {
                        zorgtoeslag += 431.00
                    }
                    else if (o[i].zorg == 4)
                    {
                        zorgtoeslag += 261.10
                    }
                    else if (o[i].zorg == 5)
                    {
                        zorgtoeslag += 431.00
                    }
                    else if (o[i].zorg == 6)
                    {
                        zorgtoeslag += 431.00
                    }
                    else if (o[i].zorg == 7)
                    {
                        zorgtoeslag += 490.07
                    }
                    else if (o[i].zorg == 8)
                    {
                        zorgtoeslag += 525.08
                    }
                    else if (o[i].zorg == 9)
                    {
                        zorgtoeslag += 560.08
                    }
                }

                // Sociale toeslagen

                if (o[i].inkomen > 0 && o[i].inkomen <= 30000)
                {
                    socialetoeslag += 55;

                    if (o[i].ouderinvalide == 1)
                    {
                        socialetoeslag += 10;
                    }

                    if (o[i].eenoudergezin == 1)
                    {
                        socialetoeslag += 20;
                    }

                    if (o.length >= 3)
                    {
                        socialetoeslag += 35;
                    }
                }
                if (o[i].inkomen > 30000 && inkomen < 50000)
                {
                    socialetoeslag += 25;

                    if (o[i].ouderinvalide == 1)
                    {
                        socialetoeslag += 0;
                    }

                    if (o[i].eenoudergezin == 1)
                    {
                        socialetoeslag += 10;
                    }

                    if (o.length >= 3)
                    {
                        socialetoeslag += 20;
                    }
                }

                // Schooltoeslag

                if (o[i].leeftijd >= 0 && o[i].leeftijd <= 5)
                {
                    schooltoeslag = 20;
                }
                else if (o[i].leeftijd >= 6 && o[i].leeftijd <= 11)
                {
                    schooltoeslag = 30;
                }
                else if (o[i].leeftijd >= 12 && o[i].leeftijd <= 17)
                {
                    schooltoeslag = 50;
                }
                else if (o[i].leeftijd >= 18 && o[i].leeftijd <= 24)
                {
                    schooltoeslag = 80;
                }

                // kindergeld 

                k = basis + leeftijdstoeslag + zorgtoeslag + socialetoeslag + eenoudertoeslag;

                kindergeld += k;
                detail.push({ geboortejaar: geboortejaar, kindergeld: k, geboortepremie: geboortepremie, schooltoeslag: schooltoeslag });
            }
        }
    }

    ret.kindergeld = kindergeld;
    ret.detail = detail;

    return (ret);
}

//---SORT ARRAY OBJECT-----------------------------------------------------------------------------------------------------------------------------------------------------------------------

Array.prototype.sortBy = function (p)
{
    return this.slice(0).sort(function (a, b)
    {
        return (a[p] < b[p]) ? 1 : (a[p] > b[p]) ? -1 : 0;
    });
}

ï»¿"use strict"

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  BEREKEN NETTOLOON
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenNetto(mGewest, mStatuut, mBS, mPartnerHasInkomen, mKTL, m65TL, mAPTL, mMaandloon, mVerdienerIsInvalide, mPartnerIsInvalide, mPartnerNettoloon, mPartnerNettopensioen, mVergoedingenAmbtenaar, mBeroepskosten, mBeheerskostenProcent, mOptieZelfstandige)
{
    var oRet;
    oRet = berekenLoon(mGewest, mStatuut, mBS, mPartnerHasInkomen, mKTL, m65TL, mAPTL, mMaandloon, mVerdienerIsInvalide, mPartnerIsInvalide, mPartnerNettoloon, mPartnerNettopensioen, mVergoedingenAmbtenaar, mBeroepskosten, mBeheerskostenProcent, mOptieZelfstandige)
    return (oRet)
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  BEREKEN BRUTOLOON
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenBruto(mGewest, mStatuut, mBS, mPartnerHasInkomen, mKTL, m65TL, mAPTL, mMaandloon, mVerdienerIsInvalide, mPartnerIsInvalide, mPartnerNettoloon, mPartnerNettopensioen, mVergoedingenAmbtenaar, mBeroepskosten, mBeheerskostenProcent, mOptieZelfstandige)
{
    var oRet;
    var xResultaat = 0;
    var xMin = 0;
    var xMax = 0;
    var xGuess = 0;
    var xGo = true;
    var xTeller = 0;

    xResultaat = 0;
    xMin = mMaandloon;
    xMax = mMaandloon * 3;
    xGuess = xMin + ((xMax - xMin) / 2);

    do
    {
        oRet = berekenLoon(mGewest, mStatuut, mBS, mPartnerHasInkomen, mKTL, m65TL, mAPTL, xGuess, mVerdienerIsInvalide, mPartnerIsInvalide, mPartnerNettoloon, mPartnerNettopensioen, mVergoedingenAmbtenaar, mBeroepskosten, mBeheerskostenProcent, mOptieZelfstandige);
        xResultaat = roundit(oRet.Netto, 3);

        if (xResultaat == mMaandloon)
        {
            xGo = false;
        }
        else if (xResultaat < mMaandloon)
        {
            xMin = xGuess;
            xGuess = xMin + ((xMax - xMin) / 2);
        }
        else if (xResultaat > mMaandloon)
        {
            xMax = xGuess;
            xGuess = xMin + ((xMax - xMin) / 2);
        }

        xTeller++;
    }
    while (xGo == true || xTeller < 100)

    xGuess = roundit(xGuess, 2);
    oRet.Bruto = xGuess;

    return (oRet);
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  BEREKEN LOON VAN BRUTO NAAR NETTO = default berekening - van netto naar bruto = via iteratie
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenLoon(vGewest, vStatuut, vBS, vPartnerHasInkomen, vKTL, v65TL, vAPTL, vMaandloon, vVerdienerIsInvalide, vPartnerIsInvalide, vPartnerNettoloon, vPartnerNettopensioen, vVergoedingenAmbtenaar, vKosten, vBeheerskostenProcent, vOptieZelfstandige)
{
    var t = "";

    // Hulpvariabelen

    var n;
    var x = 0;
    var y = 0;
    var v1 = 0;
    var v2 = 0;
    var b1 = 0;
    var b2 = 0;

    // Te gebruiken cijfers

    var vBelastingVerminderingMetInkomen = 2097.20;
    var vBelastingVerminderingZonderInkomen = 4194.40;
    var vMaximumToekenningAanPartner = 11000;

    var vVerminderingAlleenstaande = 312;
    var vVerminderingAlleenstaandeMetKtl = 444;
    var vVerminderingAlleenstaandeHandicap = 444;
    var vVerminderingAlleenstaande65tl = 972;
    var vVerminderingAlleenstaandeAtl = 444;
    var vVerminderingAlleenstaandeLaagInkomen1 = 1398;
    var vVerminderingAlleenstaandeLaagInkomen2 = 2796;

    var vVerminderingHandicap = 444;
    var vVerminderingHandicapPartner = 444;
    var vVermindering65tl = 972;
    var vVerminderingAtl = 444;

    var vMaximumInkomenPartnerPensioenenOfRenteVoorBerekeningBasisBelasting = 140;

    var vMaximumInkomenPartnerPensioenenOfRenteGrens1 = 233;
    var vMaximumInkomenPartnerPensioenenOfRenteGrens2 = 466;

    //

    var vRSZ = 0;
    var vWerkbonus = 0;
    var vBeroepskosten = 0;
    var vBelastbaarMaandinkomen = 0;
    var vBelastbaarJaarinkomen = 0;
    var vBasisbelasting = 0;
    var vVermindering = 0;
    var vBVJaar = 0;
    var vBVMaand = 0;
    var vBBSZ = 0;
    var vNettoloon = 0;

    var vRSZ_Basis = 0;
    var vRSZ_Zelfstandige = 0;
    var vRSZ_Zelfstandige_Maand = 0;

    //

    var oResult = {
        Bruto: 0,
        RSZ: 0,
        Werkbonus: 0,
        BelastbaarMaandinkomen: 0,
        BV: 0,
        BBSZ: 0,
        Netto: 0
    };

    // Bereken sociale bijdragen

    vRSZ = berekenRSZ(vMaandloon, vStatuut);

    // Bereken werkbonus

    vWerkbonus = berekenWerkbonus(vMaandloon, vStatuut);

    if (vWerkbonus > vRSZ)
    {
        vWerkbonus = vRSZ;
    }

    // belastbaar ...					

    vBelastbaarMaandinkomen = vMaandloon - vRSZ + vWerkbonus;

    // afronden op het lagere veelvoud van 15 en naar jaar omzetten

    y = ((Math.floor(vBelastbaarMaandinkomen / 15)) * 15) * 12;

    // Beroepskosten in mindering brengen ...

    vBeroepskosten = berekenBeroepskosten(y, vStatuut);	    				// vStatuut: 1=arbeider 2=bediende 3=ambtenaar 4=zelfstandige 5=bedrijfsleider

    // berekening belastbaar jaarinkomen

    vBelastbaarJaarinkomen = y - vBeroepskosten;

    // jaarbelasting berekenen                                               // vBS: 0 = alleenstaande 1 = Gehuwd 2 = Wettelijk samenwonend 

    if (vBS == 0)
    {
        vBasisbelasting = roundit(berekenBasisBelasting(vBelastbaarJaarinkomen) - vBelastingVerminderingMetInkomen, 2);
    }
    else if ((vBS == 1 || vBS == 2) && vPartnerHasInkomen == 1)
    {
        if (vPartnerNettoloon == 0 && vPartnerNettopensioen <= vMaximumInkomenPartnerPensioenenOfRenteVoorBerekeningBasisBelasting)
        {
            v1 = Math.min(vBelastbaarJaarinkomen * 0.30, vMaximumToekenningAanPartner);			// aan andere partner
            v2 = vBelastbaarJaarinkomen - v1;

            b1 = roundit(berekenBasisBelasting(v1), 2);
            b2 = roundit(berekenBasisBelasting(v2), 2);

            vBasisbelasting = roundit(b1 + b2 - vBelastingVerminderingZonderInkomen, 2);
        }
        else
        {
            vBasisbelasting = roundit(berekenBasisBelasting(vBelastbaarJaarinkomen) - vBelastingVerminderingMetInkomen, 2);
        }
    }
    else if ((vBS == 1 || vBS == 2) && vPartnerHasInkomen == 0)
    {
        v1 = Math.min(vBelastbaarJaarinkomen * 0.30, vMaximumToekenningAanPartner);			// aan andere partner
        v2 = vBelastbaarJaarinkomen - v1;

        b1 = roundit(berekenBasisBelasting(v1), 2);
        b2 = roundit(berekenBasisBelasting(v2), 2);

        vBasisbelasting = roundit(b1 + b2 - vBelastingVerminderingZonderInkomen, 2);
    }

    //
    // Verminderingen verrekenen 
    //

    // Kinderen ten laste

    vVermindering = 0;
    vVermindering = berekenVerminderingKTL(vKTL);

    // Andere verminderingen

    if ((vBS == 1 || vBS == 2) && vPartnerHasInkomen == 0)		// Gehuwd en partner heeft geen inkomsten
    {
        if (vVerdienerIsInvalide == 1)						    // Verdiener is mindervalide
        {
            vVermindering = vVermindering + vVerminderingHandicap;
        }

        if (vPartnerIsInvalide == 1)							// Echtgenoot is mindervalide
        {
            vVermindering = vVermindering + vVerminderingHandicapPartner;
        }

        vVermindering = vVermindering + (v65TL * vVermindering65tl);
        vVermindering = vVermindering + (vAPTL * vVerminderingAtl);
    }
    else													    // In de andere gevallen ...
    {
        if (vBS == 0)										    // Alleenstaande
        {
            vVermindering = vVermindering + vVerminderingAlleenstaande;
        }

        if (vBS == 0 && vKTL > 0)							    // Alleenstaande met kinderen ten laste
        {
            vVermindering = vVermindering + vVerminderingAlleenstaandeMetKtl;
        }

        if (vVerdienerIsInvalide == 1)						    // Verdiener is mindervalide
        {
            vVermindering = vVermindering + vVerminderingAlleenstaandeHandicap;
        }

        vVermindering = vVermindering + (v65TL * vVerminderingAlleenstaande65tl);		    // Personen ten laste 65+
        vVermindering = vVermindering + (vAPTL * vVerminderingAlleenstaandeAtl);		    // Andere personen ten laste 

        if (vBS != 0 && vPartnerNettoloon <= vMaximumInkomenPartnerPensioenenOfRenteGrens1 && vPartnerNettopensioen == 0)
        {
            vVermindering = vVermindering + vVerminderingAlleenstaandeLaagInkomen1		    // Grens laag inkomen
        }

        if (vBS != 0 && vPartnerNettoloon == 0 && vPartnerNettopensioen <= vMaximumInkomenPartnerPensioenenOfRenteGrens2)
        {
            vVermindering = vVermindering + vVerminderingAlleenstaandeLaagInkomen2 		    // Grens laag inkomen
        }
    }

    //// vermindering voor lage inkomens overheidssector ...

    //if (vStatuut == 3)
    //{
    //    if (vBelastbaarMaandinkomen > 597.08 && vBelastbaarMaandinkomen <= 2191.18)
    //    {
    //        vVermindering = vVermindering + 70
    //    }
    //}

    // vermindering voor werkbonus

    if (vStatuut == 1 || vStatuut == 2)
    {
        if (vWerkbonus > 0)
        {
            vVermindering = vVermindering + ((vWerkbonus * 12) * 0.3314);
        }
    }

    //	

    vVermindering = roundit(vVermindering, 2)

    // voorheffing

    if (vBasisbelasting > 0)
    {
        vBVJaar = vBasisbelasting - roundit(vVermindering, 2);

        if (vBVJaar > 0)
        {
            vBVMaand = vBVJaar / 12;
            vBVMaand = (int(vBVMaand * 100)) / 100;
        }
        else
        {
            vBVJaar = 0;
            vBVMaand = 0;
        }
    }
    else
    {
        vBVJaar = 0;
        vBVMaand = 0;
    }

    //

    if (vBVMaand < 0)
    {
        vBVMaand = 0;
    }

    // bijzondere bijdrage SZ ...

    if (vStatuut == 1)
    {
        vBBSZ = berekenBBSZ(vMaandloon * 1.08, vPartnerHasInkomen);
    }
    else if (vStatuut == 2 || vStatuut == 3)
    {
        vBBSZ = berekenBBSZ(vMaandloon, vPartnerHasInkomen);
    }
    else if (vStatuut == 4 || vStatuut == 5)
    {
        vBBSZ = 0;
    }

    vBBSZ = roundit(vBBSZ, 2);

    //
    // Nettoloon per statuut ...
    //

    if (vStatuut == 1 || vStatuut == 2 || vStatuut == 3)
    {
        vNettoloon = roundit(vMaandloon - vRSZ + vWerkbonus - vBVMaand - vBBSZ, 2);

        // ambtenaar, vergoedingen bijtellen ...

        if (vStatuut == 3)
        {
            vNettoloon = vNettoloon + vVergoedingenAmbtenaar;
        }
    }
    else if (vStatuut == 4 || vStatuut == 5)    
    {
        if (vOptieZelfstandige == 0)            // Nettoloon type sleutelformule : berekening belasting
        {
            vNettoloon = roundit(vMaandloon - vBVMaand, 2);
        }
        else if (vOptieZelfstandige == 1)       // Nettoloon reel : berekening belasting + sociale bijdragen en brutering
        {
            var i = 0;
            var basis = 0;

            var bijdrage; 

            vBeroepskosten = berekenBeroepskostenZelfstandige(vMaandloon * 12, vKosten);
            vRSZ_Basis = (vMaandloon * 12) - vBeroepskosten;

            bijdrage = berekenSocialeBijdragenZelfstandige(vRSZ_Basis, 4, 0);

            vRSZ_Zelfstandige = bijdrage.jaarbijdrage;
            vRSZ_Zelfstandige_Maand = bijdrage.maandbijdrage;

            for (i = 0; i <= 10; i++)
            {
                basis = vRSZ_Basis - vRSZ_Zelfstandige;

                bijdrage = berekenSocialeBijdragenZelfstandige(basis, 4, 0);

                vRSZ_Zelfstandige = bijdrage.jaarbijdrage;
                vRSZ_Zelfstandige_Maand = bijdrage.maandbijdrage;
            }

            vRSZ = vRSZ_Zelfstandige_Maand;

            vBelastbaarMaandinkomen = roundit(vRSZ_Basis / 12, 2);
            vNettoloon = vMaandloon - (vBeroepskosten / 12) - vRSZ - vBVMaand;
        }
    }

    //

    oResult.Statuut = vStatuut;
    oResult.Bruto = vMaandloon;
    oResult.RSZ = vRSZ;
    oResult.Werkbonus = vWerkbonus;
    oResult.Beroepskosten = vBeroepskosten;
    oResult.BelastbaarMaandinkomen = vBelastbaarMaandinkomen;
    oResult.BV = vBVMaand;
    oResult.BBSZ = vBBSZ;
    oResult.Netto = vNettoloon;

    return (oResult)
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  RSZ 
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenRSZ(xInkomen, xStatuut)
{
    var ret = 0;

    // xStatuut: 1=arbeider 2=bediende 3=ambtenaar 4=zelfstandige 5=bedrijfsleider

    if (xStatuut == 1)
    {
        ret = roundit((xInkomen * 1.08) * 0.1307, 2);
    }
    else if (xStatuut == 2)
    {
        ret = roundit(xInkomen * 0.1307, 2);
    }
    else if (xStatuut == 3)
    {
        ret = (xInkomen * 0.075) + (xInkomen * 0.0355);
    }
    else if (xStatuut == 4 || xStatuut == 5)
    {
        if (xInkomen <= 1190)
        {
            ret = 330;
        }
        else if (xInkomen > 1190 && xInkomen <= 5140)
        {
            ret = 330 + ((xInkomen - 1190) * 0.215);
        }
        else if (xInkomen > 5140.01 && xInkomen <= 7400)
        {
            ret = 1179.25 + ((xInkomen - 5140) * 0.1450);
        }
        else if (xInkomen > 7560)
        {
            ret = 1530.15;
        }
    }

    return (ret)
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  BEROEPSKOSTEN
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenBeroepskosten(xInkomen, xStatuut)
{
    var ret = 0;

    // xStatuut: 1=arbeider 2=bediende 3=ambtenaar 4=zelfstandige 5=bedrijfsleider

    if (xStatuut == 1 || xStatuut == 2 || xStatuut == 3)
    {
        if (xInkomen <= 16300)
        {
            ret = xInkomen * 0.30;
        }
        else if (xInkomen > 16300)
        {
            ret = 4890;
        }
    }
    else if (xStatuut == 4 || xStatuut == 5)
    {
        if (xInkomen <= 86000)
        {
            ret = xInkomen * 0.03;
        }
        else if (xInkomen > 86000)
        {
            ret = 2580;
        }
    }

    return (ret)
}

function berekenBeroepskostenZelfstandige(xInkomen, xKosten)
{
    var ret = 0;

    if (xKosten > 100) 
    {
        ret = xKosten;
    }
    else if (xKosten > 0 && xKosten <= 100)
    {
        ret = xInkomen * (xKosten / 100);
    }

    return (ret)
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  BASISBELASTING
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenBasisBelasting(xInkomen)
{
    var ret = 0;

    if (xInkomen >= 0 && xInkomen <= 13050)
    {
        ret = xInkomen * 0.2675;
    }
    else if (xInkomen > 13050 && xInkomen <= 19920)
    {
        ret = 3490.88 + ((xInkomen - 13050) * 0.4280);
    }
    else if (xInkomen > 19920 && xInkomen <= 41070)
    {
        ret = 6431.24 + ((xInkomen - 19920) * 0.4815);
    }
    else if (xInkomen > 41070)
    {
        ret = 16614.97 + ((xInkomen - 41070) * 0.5350);
    }

    return (ret) 
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  VERMINDERING KINDEREN TEN LASTE
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenVerminderingKTL(xAantal)
{
    var ret = 0;

    switch (xAantal)
    {
        case 0:
            ret = 0;
            break
        case 1:
            ret = 444;
            break
        case 2:
            ret = 1272;
            break
        case 3:
            ret = 3360;
            break
        case 4:
            ret = 5880;
            break
        case 5:
            ret = 8688;
            break
        case 6:
            ret = 11484;
            break
        case 7:
            ret = 14292;
            break
        case 8:
            ret = 17376;
            break
    }

    if (xAantal > 8)
    {
        ret = 17376 + ((xAantal - 8) * 3120);
    }

    return (ret)
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  BIJZONDERE BIJDRAGE SOCIALE ZEKERHEID
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenBBSZ(xBedrag, xPartnerHasInkomen)
{
    var ret = 0;

    if (xPartnerHasInkomen == 1)
    {
        if (xBedrag < 1095.09)
        {
            ret = 0;
        }
        else if (xBedrag >= 1095.09 && xBedrag < 1945.38)
        {
            ret = 9.30;
        }
        else if (xBedrag >= 1945.38 && xBedrag < 2190.18)
        {
            ret = (xBedrag - 1945.38) * 0.076;
            ret = ret + 9.30; 
        }
        else if (xBedrag >= 2190.18 && xBedrag < 6038.82)
        {
            ret = (xBedrag - 2190.18) * 0.011;
            ret = ret + 18.60;

            if (ret > 51.64)
            {
                ret = 51.64;
            }
        }
        else if (xBedrag >= 6038.82)
        {
            ret = 51.64;
        }
    }
    else if (xPartnerHasInkomen == 0)
    {
        if (xBedrag < 1945.38)
        {
            ret = 0;
        }
        else if (xBedrag >= 1945.38 && xBedrag < 2190.18)
        {
            ret = (xBedrag - 1945.38) * 0.076;
        }
        else if (xBedrag >= 2190.18 && xBedrag < 6038.82)
        {
            ret = (xBedrag - 2190.18) * 0.011;
            ret = ret + 18.60;
        }
        else if (xBedrag >= 6038.82)
        {
            ret = 60.94;
        }
    }

    return (roundit(ret, 2))
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  WERKBONUS
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenWerkbonus(xReferteLoon, xStatuut)
{
    var ret = 0;

    var w1 = 0;
    var w2 = 0;

    if (xStatuut == 1)		// Arbeider
    {
        if (xReferteLoon == 0)
        {
            ret = 0;
        }
        else if (xReferteLoon <= 1641.62)
        {
            ret = 217.75;
        }
        else if (xReferteLoon > 1641.47 && xReferteLoon <= 2560.57)
        {
            ret = 217.75 - (.23709 * (xReferteLoon - 1641.62));
        }
        else if (xReferteLoon > 2560.57)
        {
            ret = 0;
        }
    }
    else if (xStatuut == 2)		// Bediende 
    {
        if (xReferteLoon == 0)
        {
            ret = 0;
        }
        else if (xReferteLoon <= 1641.62)
        {
            ret = 201.62;
        }
        else if (xReferteLoon > 1641.62 && xReferteLoon <= 2560.57)
        {
            ret = 201.62 - (0.2194 * (xReferteLoon - 1641.62));
        }
        else if (xReferteLoon > 2560.57)
        {
            ret = 0;
        }
    }
    else
    {
        ret = 0;
    }

    return roundit(ret, 2)
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  RSZ + SOLIDARITEIT PENSIOEN
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenRSZPensioen(wBeroepsStatuut, wGehuwdSamenwonend, wPartnerInkomen, wBrutoMaandPensioen)
{
    var xRSZ = 0;
    var xGrens = 0;
    var xBeperking = 0;
    var xPensioenType = 0;

    xPensioenType = getPensioenType(wGehuwdSamenwonend, wPartnerInkomen)

    // wBeroepsStatuut 1=arbeider  2=bediende  3=ambtenaar 4=zelfstandige 5=bedrijfsleider	

    if (wBeroepsStatuut == 1 || wBeroepsStatuut == 2 || wBeroepsStatuut == 5)
    {
        if (xPensioenType == 1)
        {
            if (wBrutoMaandPensioen < 1530.38)
            {
                xRSZ = 0;
            }
            else if (wBrutoMaandPensioen >= 1530.38 && wBrutoMaandPensioen <= 151586.69)
            {
                xRSZ = (wBrutoMaandPensioen - 1530.38) * 0.0355;
            }
            else if (wBrutoMaandPensioen >= 1586.69)
            {
                xRSZ = wBrutoMaandPensioen * 0.0355;
            }
        }
        else if (xPensioenType == 2)
        {
            if (wBrutoMaandPensioen < 1813.72)
            {
                xRSZ = 0;
            }
            else if (wBrutoMaandPensioen >= 1813.72 && wBrutoMaandPensioen <= 1880.46)
            {
                xRSZ = (wBrutoMaandPensioen - 1813.72) * 0.0355;
            }
            else if (wBrutoMaandPensioen >= 1880.46)
            {
                xRSZ = wBrutoMaandPensioen * 0.0355;
            }
        }
    }
    else if (wBeroepsStatuut == 3)
    {
        xRSZ = wBrutoMaandPensioen * 0.0355;
    }
    else if (wBeroepsStatuut == 4)
    {
        xRSZ = wBrutoMaandPensioen * 0.0355;
    }

    return (xRSZ)
}

//	

function berekenSolidairiteitPensioen(wBeroepsStatuut, wGehuwdSamenwonend, wPartnerInkomen, wBrutoMaandPensioen)
{
    var w = 0;
    var xPensioenType = 0;

    xPensioenType = getPensioenType(wGehuwdSamenwonend, wPartnerInkomen)

    if (xPensioenType == 1)
    {
        if (wBrutoMaandPensioen >= 2594.46 && wBrutoMaandPensioen <= 2674.68)
        {
            w = (wBrutoMaandPensioen - 2594.45) * 0.50;
        }
        else if (wBrutoMaandPensioen >= 2674.69 && wBrutoMaandPensioen <= 2873.57)
        {
            w = wBrutoMaandPensioen * 0.015;
        }
        else if (wBrutoMaandPensioen >= 2873.58 && wBrutoMaandPensioen <= 2903.51)
        {
            w = 43.10 + ((wBrutoMaandPensioen - 2873.57) * 0.50);
        }
        else if (wBrutoMaandPensioen >= 2903.52)
        {
            w = wBrutoMaandPensioen * 0.02;
        }
    }
    else if (xPensioenType == 2)
    {
        if (wBrutoMaandPensioen >= 2999.51 && wBrutoMaandPensioen <= 3092.26)
        {
            w = (wBrutoMaandPensioen - 2999.51) * 0.50;
        }
        else if (wBrutoMaandPensioen >= 3092.27 && wBrutoMaandPensioen <= 3287.31)
        {
            w = wBrutoMaandPensioen * 0.015;
        }
        else if (wBrutoMaandPensioen >= 3287.32 && wBrutoMaandPensioen <= 3321.55)
        {
            w = 49.31 + ((wBrutoMaandPensioen - 3287.31) * 0.50);
        }
        else if (wBrutoMaandPensioen >= 3321.56)
        {
            w = wBrutoMaandPensioen * 0.02;
        }
    }

    // Niet voor zelfstandige en bedrijfsleider

    if (wBeroepsStatuut == 4 || wBeroepsStatuut == 5)
    {
        w = 0;
    }

    //

    return (w)
}

// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//  SOCIALE BIJDRAGEN ZELFSTANDIGE
// ------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenSocialeBijdragenZelfstandige(wInkomen, wBeheerskosten, wIndexatieYorN)
{
    var i = 0;

    var xCoeffIndexatie = 1.0522372;

    // Indexering enkel toepassen indien inkomen het inkomen van 3 jaar geleden is, anders niet!!!

    var xSchijfEen = 13993.78;
    var xSchijfTwee = 60427.75;
    var xSchijfDrie = 89051.37;

    var xProcentSchijfTwee = 5.125;
    var xProcentSchijfDrie = 3.54;

    var xMinimumBijdrage = 739.05;
    var xMaximumBijdrage = 4235.56;

    var xBeheersKostenProcent = 0.0305;

    //

    var xInkomen = 0;
    var xBasisBijdrage = 0
    var xBeheersKosten = 0;
    var xKwartaalBijdrage = 0;
    var xMaandBijdrage = 0;

    var ret = { maandbijdrage: 0, kwartaalbijdrage: 0, jaarbijdrage: 0, beheerskosten: 0 }

    //	

    var xInkomen = wInkomen;

    //
 
    if (wIndexatieYorN == 1)
    {
        xInkomen = xInkomen * xCoeffIndexatie;
    }

    //

    if (xInkomen <= xSchijfEen)
    {
        xBasisBijdrage = xMinimumBijdrage;
        xBeheersKosten = 0;
        xKwartaalBijdrage = xBasisBijdrage + xBeheersKosten;
    }
    else if (xInkomen > xSchijfEen && xInkomen <= xSchijfTwee)
    {
        xBasisBijdrage = xInkomen * (xProcentSchijfTwee / 100);
        xBeheersKosten = xBasisBijdrage * (xBeheersKostenProcent / 100);
        xKwartaalBijdrage = xBasisBijdrage + xBeheersKosten;
    }
    else if (xInkomen > xSchijfTwee)
    {
        var b1 = xSchijfTwee * (xProcentSchijfTwee / 100);
        var b2 = (xInkomen - xSchijfTwee) * (xProcentSchijfDrie / 100);

        xBasisBijdrage = b1 + b2;

        if (xBasisBijdrage > xMaximumBijdrage)
        {
            xBasisBijdrage = xMaximumBijdrage;
            xKwartaalBijdrage = xMaximumBijdrage;
            xBeheersKosten = 0;
        }
        else 
        {
            xBeheersKosten = xBasisBijdrage * (xBeheersKostenProcent / 100);
            xKwartaalBijdrage = xBasisBijdrage + xBeheersKosten;

            if (xKwartaalBijdrage > xMaximumBijdrage)
            {
                xKwartaalBijdrage = xMaximumBijdrage;
            }
        }
    }

    // Omzetten naar een maandbijdrage

    ret.maandbijdrage = xKwartaalBijdrage / 3;
    ret.kwartaalbijdrage = xKwartaalBijdrage;
    ret.jaarbijdrage = xKwartaalBijdrage * 4;
    ret.beheerskosten = xBeheersKosten;

    return (ret)
}


ï»¿
//------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenPensioenWerknemer(xLeeftijd, xLeeftijdAanvang, xLeeftijdPensioen, xStatuut, xMaandLoon, xMaandLoonFuture, xMaandLoonCoefficient, xPartTime, xExtraInkomen, xGehuwdSamenwonend, xPartnerInkomen)
{
    var xBreuk = 0;
    var xProcent = 0;

    var xPensioen = 0;
    var xMinimumPensioen = 0;
    var xMaximumPensioen = 0;

    var xMinimumLeeftijdPensioen = 0;
    var xLeeftijdEindeLoopbaan = 0;

    var xJaarVanPensioen = 0;
    var xJaar = 0

    var xToday = new Date();

    var xGrens1 = 0;
    var xGrens2 = 0;
    var xGrens3 = 0;

    var xAantalMaanden = 0;
    var xAantalDagenPerMaand = 0;

    var xPensioenOK = true;

    // tellers ...

    var i = 0;
    var j = 0;

    //

    var m = 0;
    var n = 0;
    var o = 0;
    var p = 0;

    var x1 = 0;
    var x2 = 0;
    var x3 = 0;
    var x4 = 0;

    var t = "";
    var tx = "";

    var correctie = 0;

    //
    // Welk pensioen: gezins- of alleenstaande pensioen
    //

    if (xGehuwdSamenwonend == 0)
    {
        xProcent = 0.60;
        xMinimumPensioen = 1236.55;
        xMaximumPensioen = 2456.37;
    }
    else if (xGehuwdSamenwonend == 1 && xPartnerInkomen == 1)
    {
        xProcent = 0.60;
        xMinimumPensioen = 1236.55;
        xMaximumPensioen = 2456.37;
    }
    else if (xGehuwdSamenwonend == 1 && xPartnerInkomen == 0)
    {
        xProcent = 0.75
        xMinimumPensioen = 1545.20;
        xMaximumPensioen = 3070.46;
    }

    //
    // Controles 
    //

    if (xMaandLoon == 0)
    {
        xPensioenOK = false;
    }

    if (xLeeftijd < 18)
    {
        xPensioenOK = false;
    }

    if (xLeeftijdAanvang < 18)
    {
        xPensioenOK = false;
    }

    if (xLeeftijdPensioen < xLeeftijd)
    {
        xPensioenOK = false;
    }

    //

    xMinimumLeeftijdPensioen = getMinimumPensioenLeeftijd(xLeeftijdAanvang)

    if (xMinimumLeeftijdPensioen > 65)
    {
        xMinimumLeeftijdPensioen = 65;
    }

    //

    if (xLeeftijdPensioen < xMinimumLeeftijdPensioen)
    {
        correctie = xMinimumLeeftijdPensioen - xLeeftijdPensioen;
        xLeeftijdEindeLoopbaan = xMinimumLeeftijdPensioen;
    }
    else if (xLeeftijdPensioen >= xMinimumLeeftijdPensioen)
    {
        correctie = 0;
        xLeeftijdEindeLoopbaan = xLeeftijdPensioen;
    }

    //

    if (xPensioenOK == true)
    {
        // loopbaan
        m = xLeeftijdEindeLoopbaan - xLeeftijdAanvang;

        // aantal jaren reeds gewerkt 
        n = xLeeftijd - xLeeftijdAanvang

        // aantal jaren nog te werken 
        o = xLeeftijdEindeLoopbaan - xLeeftijd

        //	

        if (m > 0)
        {
            initPensioenTabellen()

            //
            // Tabellen maken ...
            //

            var statuut = [];

            for (i = 1; i <= m; i++)
            {
                if (xStatuut == 1)
                {
                    statuut[i] = "A"    // arbeider
                }
                else if (xStatuut == 2)
                {
                    statuut[i] = "B"    // bediende
                }
            }

            //
            //

            var jaar = [];
            var leeftijd = [];

            x1 = xToday.getFullYear();
            x2 = x1 - (xLeeftijd - xLeeftijdAanvang);

            for (i = 0; i <= m; i++)
            {
                jaar[i + 1] = x2 + i;
                leeftijd[i + 1] = xLeeftijdAanvang + i;
            }

            //
            // Loopbaanbreuk
            //

            var breuk = [];

            for (i = 1; i <= m; i++)
            {
                xBreuk = 45;
                breuk[i] = 45;
            }

            //
            // Tabel met maximumloon ...
            //

            var maxloon = [];

            for (i = 1; i <= m; i++)
            {
                for (j = 1; j <= ML.length; j++)
                {
                    tx = String(ML[j])

                    if (jaar[i] == tx.substr(0, 4))
                    {
                        maxloon[i] = roundit(parseFloat(tx.substr(5)) * (xPartTime / 100), 2)
                    }
                }
            }

            tx = String(ML[ML.length - 1]);

            for (i = n + 1; i <= xLeeftijdEindeLoopbaan; i++)
            {
                maxloon[i] = parseFloat(tx.substr(5));
            }

            //
            // Tabel met maandloon ...
            //

            var loon = [];

            // toekomstig loon

            if (xMaandLoonFuture > 0)
            {
                for (i = n; i <= xLeeftijdEindeLoopbaan; i++)
                {
                    loon[i] = xMaandLoonFuture * xMaandLoonCoefficient;
                }
            }
            else
            {
                for (i = n; i <= xLeeftijdPensioen; i++)
                {
                    loon[i] = xMaandLoon * xMaandLoonCoefficient;
                }
            }

            // nu

            loon[n] = xMaandLoon * xMaandLoonCoefficient;

            // verleden 

            x1 = xMaandLoon * xMaandLoonCoefficient;
            x2 = roundit(x1 / parseFloat(maxloon[n]), 4)

            for (i = 1; i < n; i++)
            {
                loon[i] = roundit(parseFloat(maxloon[i]) * x2, 2)
            }

            //
            // Herwaarderingscoefficient
            //

            var herw = [];

            for (i = 1; i <= m; i++)
            {
                for (j = 1; j <= HW.length; j++)
                {
                    tx = String(HW[j])

                    if (jaar[i] == tx.substr(0, 4))
                    {
                        herw[i] = parseFloat(tx.substr(5))
                    }
                }
            }

            tx = String(HW[HW.length - 1]);

            for (i = n + 1; i <= xLeeftijdEindeLoopbaan; i++)
            {
                herw[i] = parseFloat(tx.substr(5));
            }

            //
            //
            //

            var pensioen = [];

            x1 = 0

            for (i = 1; i <= m; i++)
            {

                if (xStatuut == 1)		// arbeider ...
                {
                    if (parseFloat(jaar[i]) <= 1980)
                    {
                        x1 = parseFloat(loon[i])
                    }
                    else
                    {
                        x1 = Math.min(parseFloat(loon[i]), parseFloat(maxloon[i]))
                    }
                }
                else if (xStatuut == 2)			// bediende ...
                {
                    x1 = Math.min(parseFloat(loon[i]), parseFloat(maxloon[i]))
                }

                pensioen[i] = roundit(x1 * (1 / parseFloat(breuk[i])) * parseFloat(herw[i]) * xProcent, 2);

            }

            //
            // Correctie indien vroeger met pensioen
            //

            if (correctie > 0)
            {
                for (i = 1; i <= m; i++)
                {
                    if (leeftijd[i] >= xLeeftijdPensioen)
                    {
                        pensioen[i] = 0;
                    }
                }
            }

            //
            // Pensioen berekenen
            //

            xPensioen = 0;

            for (i = 1; i <= m; i++)
            {
                xPensioen += parseFloat(pensioen[i]);
            }

            xPensioen = roundit(xPensioen / 12, 2)

            //
            // Controle minimumpensioen volgens het soepele criterium
            //

            x2 = xBreuk * 2 / 3

            if ((m - correctie) >= x2)
            {
                x3 = roundit(xMinimumPensioen * ((m - correctie) / xBreuk), 2);
                xPensioen = Math.max(x3, xPensioen);
            }

            //
            // Controle maximumpensioen ...
            //

            if (xPensioen > xMaximumPensioen)
            {
                xPensioen = xMaximumPensioen
            }

            //
        }
    }

    // Tabellen ...

    //tw = ""
    //tw = tw + "<!DOCTYPE html>"
    //tw = tw + "<html xmlns='http://www.w3.org/1999/xhtml'>"

    //tw = tw + "<title>" + "" + "</title>"

    //tw = tw + "<link href='css/afdruk.css' rel=stylesheet>"
    //tw = tw + "<style TYPE='text/css'></style>"

    //tw = tw + "</head>"

    //tw = tw + "<body leftmargin ='25px'>"

    //tw = tw + "<table class='kader' style='padding: 8px;'>"

    //tw = tw + "<tr>"
    //tw = tw + "<td style='text-align:right;'>" + "Statuut" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Jaar" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Leeftijd" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Breuk" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Max. loon" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Loon" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Herw." + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Pensioen" + "</td>"
    //tw = tw + "</tr>"

    //for (i = 1; i <= m; i++) {
    //    tw = tw + "<tr>"
    //    tw = tw + "<td style='text-align:right;'>" + statuut[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + jaar[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + leeftijd[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + breuk[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + roundit(maxloon[i], 2) + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + roundit(loon[i], 2) + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + herw[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + roundit(pensioen[i], 2) + "</td>"
    //    tw = tw + "</tr>"
    //}

    //tw = tw + "<tr>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'>" + roundit(xPensioen * 12, 2) + "</td>"
    //tw = tw + "</tr>"

    //tw = tw + "<tr>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'>" + roundit(xPensioen, 2) + "</td>"
    //tw = tw + "</tr>"

    //tw = tw + "</table>"

    //tw = tw + "</body>"
    //tw = tw + "</html>"

    //doPrint(tw);

    return (xPensioen)

    //
}

//------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenPensioenZelfstandige(xLeeftijd, xLeeftijdAanvang, xLeeftijdPensioen, xMaandLoon, xMaandLoonFuture, xMaandLoonCoefficient, xExtraInkomen, xGehuwdSamenwonend, xPartnerInkomen)
{
    var xBreuk = 0;
    var xPensioen = 0;
    var xPensioenType = 0;
    var xMinimumPensioen = 0;
    var xMinimumPensioenMaand = 0;
    var xProcentVermindering = 0;
    var xMaximumPensioen = 0;
    var xPensioenBonus = 0

    var xMinimumLeeftijdPensioen = 0;
    var xLeeftijdEindeLoopbaan = 0;

    var xJaarVanPensioen = 0;
    var xJaar = 0

    var xGrens1 = 0;
    var xGrens2 = 0;
    var xGrens3 = 0;

    var xPensioenOK = true;

    var xAantalTrimesters = 0;

    // tellers ...

    var i = 0;
    var j = 0;

    //

    var m = 0;
    var n = 0;
    var o = 0;

    var x1 = 0;
    var x2 = 0;
    var x3 = 0;
    var x4 = 0;

    var t = "";
    var tx = "";
    var correctie;

    //

    var xToday = new Date();

    //
    // Welk pensioen: gezins- of alleenstaande pensioen
    //

    if (xGehuwdSamenwonend == 1 && xPartnerInkomen == 0)
    {
        xPensioenType = 2;							// Gezinspensioen
        xMinimumPensioen = 18307.17;
    }
    else
    {
        xPensioenType = 1;							// Alleenstaande
        xMinimumPensioen = 14650.34;
    }

    //
    // Controles 
    //

    if (xMaandLoon == 0)
    {
        xPensioenOK = false;
    }

    if (xLeeftijd < 16)
    {
        xPensioenOK = false;
    }

    if (xLeeftijdAanvang < 16)
    {
        xPensioenOK = false;
    }

    if (xLeeftijdPensioen < xLeeftijd)
    {
        xPensioenOK = false;
    }

    //

    xMinimumLeeftijdPensioen = getMinimumPensioenLeeftijd(xLeeftijdAanvang)

    if (xMinimumLeeftijdPensioen > 65)
    {
        xMinimumLeeftijdPensioen = 65;
    }

    //

    if (xLeeftijdPensioen < xMinimumLeeftijdPensioen)
    {
        correctie = xMinimumLeeftijdPensioen - xLeeftijdPensioen;
        xLeeftijdEindeLoopbaan = xMinimumLeeftijdPensioen;
    }
    else if (xLeeftijdPensioen >= xMinimumLeeftijdPensioen)
    {
        correctie = 0;
        xLeeftijdEindeLoopbaan = xLeeftijdPensioen;
    }

    //

    if (xPensioenOK == true)
    {
        //
        //

        initPensioenTabellen()

        // loopbaan
        m = xLeeftijdEindeLoopbaan - xLeeftijdAanvang;

        // aantal jaren reeds gewerkt 
        n = xLeeftijd - xLeeftijdAanvang

        // aantal jaren nog te werken 
        o = xLeeftijdEindeLoopbaan - xLeeftijd

        //
        //

        var statuut = [];

        for (i = 1; i <= m; i++)
        {
            statuut[i] = "zelfstandige"
        }

        //				
        //

        var jaar = [];
        var leeftijd = [];

        x1 = xToday.getFullYear();
        x2 = x1 - (xLeeftijd - xLeeftijdAanvang);

        for (i = 0; i <= m; i++)
        {
            jaar[i + 1] = x2 + i
            leeftijd[i + 1] = xLeeftijdAanvang + i;
        }

        //
        // Loopbaanbreuk
        //

        var breuk = [];

        for (i = 1; i <= m; i++)
        {
            xBreuk = 45;
            breuk[i] = 45;
        }

        //
        // Tabel met maximumloon ...
        //

        var maxloon = [];

        for (i = 1; i <= m; i++)
        {
            for (j = 1; j <= MLZ.length; j++)
            {
                tx = String(MLZ[j])

                if (parseFloat(jaar[i]) < 1984)
                {
                    maxloon[i] = 8133.63
                }
                else if (jaar[i] == tx.substr(0, 4))
                {
                    maxloon[i] = parseFloat(tx.substr(5))
                }
            }
        }

        // toekomst

        tx = String(MLZ[MLZ.length - 1]);
        for (i = n + 1; i <= xLeeftijdEindeLoopbaan; i++)
        {
            maxloon[i] = parseFloat(tx.substr(5));
        }

        //
        // Tabel met maandloon ...
        //

        var loon = [];

        // toekomstig loon

        if (xMaandLoonFuture > 0)
        {
            for (i = n; i <= xLeeftijdEindeLoopbaan; i++)
            {
                loon[i] = xMaandLoonFuture * xMaandLoonCoefficient;
            }
        }
        else if (xMaandLoonFuture == 0)
        {
            for (i = n; i <= xLeeftijdEindeLoopbaan; i++)
            {
                loon[i] = xMaandLoon * xMaandLoonCoefficient;
            }
        }

        // nu

        loon[n] = xMaandLoon * xMaandLoonCoefficient;

        // verleden 

        x1 = xMaandLoon * xMaandLoonCoefficient;
        x2 = roundit(x1 / parseFloat(maxloon[n]), 4)

        for (i = 1; i < n; i++)
        {
            if (parseFloat(jaar[i]) < 1984)
            {
                loon[i] = 8133.63
            }
            else
            {
                loon[i] = roundit(parseFloat(maxloon[i]) * x2, 2)
            }
        }

        // Berekeningscoefficient

        var herw = [];

        for (i = 1; i <= m; i++)
        {
            if (parseFloat(jaar[i]) < 1984)
            {
                tx = String(CO[1])

                if (xPensioenType == 1)					// alleenstaande
                {
                    herw[i] = tx.substr(23, 8)
                }
                else if (xPensioenType == 2)			// gezin
                {
                    herw[i] = tx.substr(14, 8)
                }
            }
            else if (parseFloat(jaar[i]) >= 1984)
            {
                for (j = 1; j <= CO.length; j++)
                {

                    tx = String(CO[j])

                    if (jaar[i] == tx.substr(0, 4))
                    {
                        x1 = parseFloat(tx.substr(5, 8))

                        if (x1 == 0)
                        {
                            if (xPensioenType == 1)							// alleenstaande
                            {
                                herw[i] = tx.substr(23, 8)
                            }
                            else if (xPensioenType == 2)					// gezin
                            {
                                herw[i] = tx.substr(14, 8)
                            }
                        }
                        else if (x1 != 0)
                        {
                            if (Math.min(parseFloat(loon[i]), parseFloat(maxloon[i])) <= x1)
                            {
                                if (xPensioenType == 1)						// alleenstaande
                                {
                                    herw[i] = tx.substr(23, 8)
                                }
                                else if (xPensioenType == 2)				// gezin
                                {
                                    herw[i] = tx.substr(14, 8)
                                }
                            }
                            else if (Math.min(parseFloat(loon[i]), parseFloat(maxloon[i])) > x1)
                            {
                                if (xPensioenType == 1)						// alleenstaande
                                {
                                    herw[i] = tx.substr(41, 8)
                                }
                                else if (xPensioenType == 2)				// gezin
                                {
                                    herw[i] = tx.substr(32, 8)
                                }
                            }
                        }
                    }
                }
            }
        }

        // toekomst

        tx = String(CO[CO.length - 1]);

        for (i = n + 1; i <= xLeeftijdEindeLoopbaan; i++)
        {
            if (Math.min(parseFloat(loon[i]), parseFloat(maxloon[i])) <= x1)
            {
                if (xPensioenType == 1)						// alleenstaande
                {
                    herw[i] = tx.substr(23, 8)
                }
                else if (xPensioenType == 2)				// gezin
                {
                    herw[i] = tx.substr(14, 8)
                }
            }
            else if (Math.min(parseFloat(loon[i]), parseFloat(maxloon[i])) > x1)
            {
                if (xPensioenType == 1)						// alleenstaande
                {
                    herw[i] = tx.substr(41, 8)
                }
                else if (xPensioenType == 2)				// gezin
                {
                    herw[i] = tx.substr(32, 8)
                }
            }
        }

        //
        //
        //

        var pensioen = [];

        x1 = 0

        for (i = 1; i <= m; i++)
        {
            if (parseFloat(jaar[i]) < 1984)
            {
                pensioen[i] = roundit(parseFloat(loon[i]) * (1 / parseFloat(breuk[i])), 2);
            }
            else
            {
                x1 = Math.min(parseFloat(loon[i]), parseFloat(maxloon[i]))
                pensioen[i] = roundit(x1 * (1 / parseFloat(breuk[i])) * parseFloat(herw[i]), 2);
            }
        }

        //
        // Correctie indien vroeger met pensioen
        //

        if (correctie > 0)
        {
            for (i = 1; i <= m; i++)
            {
                if (leeftijd[i] >= xLeeftijdPensioen)
                {
                    pensioen[i] = 0;
                }
            }
        }

        //

        x1 = 0;
        for (i = 1; i <= m; i++)
        {
            x1 = x1 + parseFloat(pensioen[i]);
        }

        xPensioen = roundit(x1 / 12, 2)

        //
        // Controle of recht op minimumpensioen ...
        //

        x2 = xBreuk * 2 / 3

        if ((m - correctie) >= x2)
        {
            x3 = roundit(xMinimumPensioen * ((m - correctie) / xBreuk), 2);
            x3 = roundit(x3 / 12, 2);
            xPensioen = Math.max(x3, xPensioen);
        }
    }

    // Tabellen ...

    //tw = ""
    //tw = tw + "<!DOCTYPE html>"
    //tw = tw + "<html xmlns='http://www.w3.org/1999/xhtml'>"

    //tw = tw + "<title>" + "" + "</title>"

    //tw = tw + "<link href='css/afdruk.css' rel=stylesheet>"
    //tw = tw + "<style TYPE='text/css'></style>"

    //tw = tw + "</head>"

    //tw = tw + "<body leftmargin ='25px'>"

    //tw = tw + "<table class='kader' style='padding: 8px;'>"


    //tw = tw + "<tr>"
    //tw = tw + "<td style='text-align:right;'>" + "Statuut" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Jaar" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Leeftijd" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Breuk" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Max. loon" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Loon" + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Herw." + "</td>"
    //tw = tw + "<td style='text-align:right;'>" + "Pensioen" + "</td>"
    //tw = tw + "</tr>"

    //for (i = 1; i <= m; i++)
    //{
    //    tw = tw + "<tr>"
    //    tw = tw + "<td style='text-align:right;'>" + statuut[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + jaar[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + leeftijd[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + breuk[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + roundit(maxloon[i], 2) + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + roundit(loon[i], 2) + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + herw[i] + "</td>"
    //    tw = tw + "<td style='text-align:right;'>" + roundit(pensioen[i], 2) + "</td>"
    //    tw = tw + "</tr>"
    //}

    //tw = tw + "<tr>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'>" + roundit(xPensioen * 12, 2) + "</td>"
    //tw = tw + "</tr>"

    //tw = tw + "<tr>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'></td>"
    //tw = tw + "<td style='text-align:right;'>" + roundit(xPensioen, 2) + "</td>"
    //tw = tw + "</tr>"

    //tw = tw + "</table>"

    ////

    //tw = tw + "</body>"
    //tw = tw + "</html>"

    //doPrint(tw)

    //	

    return (xPensioen)

}

//------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function berekenPensioenAmbtenaar(xLeeftijd, xLeeftijdAanvang, xLeeftijdPensioen, xCategorie, xMaandLoon, xMaandLoonFuture, xGehuwdSamenwonend, xPartnerInkomen)
{
    var xPensioen = 0;
    var xMinimumPensioen = 0;
    var xProcent = 0;
    var xCoef = 0;
    var xPlafond = 0;
    var xToegelatenDuur = 0
    var xPensioenBonus = 0;
    var xAantalMaanden = 0;
    var xAantalDagenPerMaand = 0;

    var xPensioenOK = true;
    var xMinimumLeeftijdPensioen = 0;
    var xJaarVanPensioen = 0;
    var xJaar = 0

    // tellers ...

    var i = 0;
    var j = 0;

    //

    var m = 0;
    var n = 0;
    var o = 0;

    var x1 = 0;
    var x2 = 0;
    var x3 = 0;
    var x4 = 0;

    var tx = "";

    var correctie;

    //

    var xToday = new Date();

    //
    // Welk pensioen: gezins- of alleenstaande pensioen
    //

    if (xGehuwdSamenwonend == 1)
    {
        xMinimumPensioen = 1707.04
    }
    else if (xGehuwdSamenwonend == 0)
    {
        xMinimumPensioen = 1365.66
    }

    //
    // Controles 
    //

    if (xMaandLoon == 0)
    {
        xPensioenOK = false;
    }

    if (xLeeftijd < 16)
    {
        xPensioenOK = false;
    }

    if (xLeeftijdAanvang < 16)
    {
        xPensioenOK = false;
    }

    if (xLeeftijdPensioen < xLeeftijd)
    {
        xPensioenOK = false;
    }

    //

    xMinimumLeeftijdPensioen = getMinimumPensioenLeeftijd(xLeeftijdAanvang)

    if (xMinimumLeeftijdPensioen > 65)
    {
        xMinimumLeeftijdPensioen = 65;
    }

    //

    if (xLeeftijdPensioen < xMinimumLeeftijdPensioen)
    {
        correctie = xMinimumLeeftijdPensioen - xLeeftijdPensioen;
    }
    else if (xLeeftijdPensioen >= xMinimumLeeftijdPensioen)
    {
        correctie = 0;
    }

    //

    xPensioen = 0;

    if (xPensioenOK == true)
    {

        // loopbaan

        m = xLeeftijdPensioen - xLeeftijdAanvang - correctie;

        //
        // Coefficienten vastleggen ...
        //

        if (xCategorie == 1)
        {
            xCoef = 60;
        }
        else if (xCategorie == 2)
        {
            xCoef = 60;
        }
        else if (xCategorie == 3)
        {
            xCoef = 55;
        }
        else if (xCategorie == 4)
        {
            xCoef = 55;
        }
        else if (xCategorie == 5)
        {
            xCoef = 60;
        }
        else if (xCategorie == 6)
        {
            xCoef = 50;
        }
        else if (xCategorie == 7)
        {
            xCoef = 60;
        }
        else if (xCategorie == 8)
        {
            xCoef = 50;
        }
        else if (xCategorie == 9)
        {
            xCoef = 60;
        }
        else if (xCategorie == 10)
        {
            xCoef = 48;
        }

        //
        //

        xToegelatenDuur = (m * 12) + 6      // op pensioen in de helft van het jaar erna ...

        if (xMaandLoonFuture > 0)
        {
            xPensioen = roundit(xMaandLoonFuture * (xToegelatenDuur / (xCoef * 12)), 2)
        }
        else if (xMaandLoonFuture == 0)
        {
            xPensioen = roundit(xMaandLoon * (xToegelatenDuur / (xCoef * 12)), 2)
        }

        //
        // Controle op minimum ...
        //

        if (m >= 20)
        {
            xPensioen = Math.max(xPensioen, xMinimumPensioen)
        }

        //
        // Controle op absoluut maximum ...
        //

        if (xPensioen * 12 > 80024.15)
        {
            xPensioen = 6668.68;
        }
    }

    return (xPensioen)
}

//------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function getMinimumPensioenLeeftijd(wLeeftijdAanvang)
{
    var wMinimumLeeftijdPensioen = 0;

    var wGrens1 = 0;
    var wGrens2 = 0;
    var wGrens3 = 0;

    //

    wGrens1 = wLeeftijdAanvang + 42

    if (wGrens1 < 63)
    {
        wGrens1 = 63
    }

    wGrens2 = wLeeftijdAanvang + 43

    if (wGrens2 < 61)
    {
        wGrens2 = 61
    }

    wGrens3 = wLeeftijdAanvang + 44

    if (wGrens3 < 60)
    {
        wGrens3 = 60
    }

    wMinimumLeeftijdPensioen = Math.min(wGrens1, wGrens2, wGrens3)

    if (wMinimumLeeftijdPensioen > 65)
    {
        wMinimumLeeftijdPensioen = 65;
    }

    return (wMinimumLeeftijdPensioen);
}

//------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function getPensioenType(xGehuwdSamenwonend, xPartnerInkomen)
{
    var retVal

    if (xGehuwdSamenwonend == 0)    // niet gehuwd ...
    {
        retVal = 1;			        // alleenstaande
    }
    else if (xGehuwdSamenwonend == 1 && xPartnerInkomen == 1)
    {
        retVal = 1;			        // alleenstaande
    }
    else if (xGehuwdSamenwonend == 1 && xPartnerInkomen == 0)
    {
        retVal = 2;			        // gezinspensioen
    }
    return (retVal)
}

//
// ---- BV PENSIOEN ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------...
//

function berekenNettoPensioen(vStatuut, vGehuwdSamenwonend, vPartnerInkomen, vPartnerInkomenBedrag, vKTL, v65TL, vAPTL, vBrutoMaandPensioen, vVerdienerInvalide, vPartnerInvalide, vOutput)
{

    var x = 0;

    var x1 = 23370;
    var x2 = 46740;

    var b1 = 0;
    var b2 = 0;

    var eenderde = 1 / 3;
    var tweederde = 2 / 3;

    var xRSZ = 0;
    var xSolidariteit = 0;
    var xBegrafenisVergoeding = 0;
    var xBelastbaarJaarinkomen = 0;
    var xBelastbaar = 0;
    var xBasisBelasting = 0;
    var xVermindering = 0;
    var xVerminderingPensioen = 0;
    var xVerminderingReferentie = 0;
    var xBVJaar = 0;
    var xBVMaand = 0;
    var xNettoPensioen = 0;

    // vStatuut: 1= arbeider  2= bediende 3= ambtenaar  4= zelfstandige	5=bedrijfsleider

    xRSZ = berekenRSZPensioen(vStatuut, vGehuwdSamenwonend, vPartnerInkomen, vBrutoMaandPensioen);
    xSolidariteit = berekenSolidairiteitPensioen(vStatuut, vGehuwdSamenwonend, vPartnerInkomen, vBrutoMaandPensioen);

    //

    xBelastbaar = vBrutoMaandPensioen - xRSZ - xSolidariteit;

    // afronden op het lagere veelvoud van 15

    x = (Math.floor(xBelastbaar / 15)) * 15;
    xBelastbaarJaarinkomen = x * 12;

    // jaarbelasting berekenen ...

    if (vGehuwdSamenwonend == 0)
    {
        xBasisBelasting = roundit(berekenBasisBelasting(xBelastbaarJaarinkomen) - 2065.10, 2);
        xVerminderingReferentie = xBelastbaarJaarinkomen;
    }
    else if (vGehuwdSamenwonend == 1 && vPartnerInkomen == 1)
    {
        xBasisBelasting = roundit(berekenBasisBelasting(xBelastbaarJaarinkomen) - 2065.10, 2);
        xVerminderingReferentie = xBelastbaarJaarinkomen;
    }
    else if (vGehuwdSamenwonend == 1 && vPartnerInkomen == 0)
    {
        v1 = Math.min(xBelastbaarJaarinkomen * 0.30, 10930);		// aan andere partner
        v2 = xBelastbaarJaarinkomen - v1;

        xVerminderingReferentie = v2;

        b1 = roundit(berekenBasisBelasting(v1), 2);
        b2 = roundit(berekenBasisBelasting(v2), 2);

        xBasisBelasting = roundit(b1 + b2 - 4130.20, 2);
    }

    // VERMINDERINGEN

    // Kinderen ten laste

    xVermindering = 0;
    xVermindering = berekenVerminderingKTL(vKTL);

    // Andere verminderingen gezinslasten = voorlopig niet weerhouden hier ...

    // Vermindering pensioenen ...

    if (xVerminderingReferentie <= x1)
    {
        xVerminderingPensioen = 2442;
    }
    else if (xVerminderingReferentie > x1 && xVerminderingReferentie < x2)
    {
        xVerminderingPensioen = (((x2 - xVerminderingReferentie) / x1) * 2442 * tweederde) + (2442 * eenderde);
    }
    else if (xVerminderingReferentie >= x2)
    {
        xVerminderingPensioen = roundit(2442 * eenderde, 2); 
    }

    // berekening netto-pensioen ...

    if (xBasisBelasting > 0)
    {
        xBVJaar = xBasisBelasting - (xVermindering + xVerminderingPensioen);

        if (xBVJaar > 0)
        {
            xBVMaand = xBVJaar / 12;
            xBVMaand = (int(xBVMaand * 100)) / 100;
        }
        else if (xBVJaar <= 0)
        {
            xBVMaand = 0;
        }
    }
    else
    {
        xBVMaand = 0;
    }

    //

    xNettoPensioen = vBrutoMaandPensioen - xRSZ - xSolidariteit - xBVMaand;

    //

    if (vStatuut == 3)
    {
        xBegrafenisVergoeding = vBrutoMaandPensioen * 0.005;
        xNettoPensioen = xNettoPensioen - xBegrafenisVergoeding;
    }

    //

    return (xNettoPensioen)
}

