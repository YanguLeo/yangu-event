//------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// ALGEMEEN
//------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function cleanNumber(val) {
    var retVal = val + "";

    if (retVal.indexOf(",") > 0 && retVal.indexOf(".") > 0) {
        retVal = replace(retVal, ".", "");
        retVal = replace(retVal, ",", ".");
    }
    else if (retVal.indexOf(",") > 0) {
        retVal = replace(retVal, ",", ".");
    }
    return retVal
}

//

function cleanNumberToFormat(val) {
    var retVal = val + "";

    if (retVal.indexOf(".") > 0) {
        retVal = replace(retVal, ".", ",");
    }
    return retVal
}

//

function formatNumber(val, dec) {
    var num = new NumberFormat();
    var retVal = ""
    num.setInputDecimal(',');
    num.setNumber(val);
    num.setPlaces(dec, false);
    num.setCurrencyValue('');
    num.setCurrency(false);
    num.setCurrencyPosition(num.LEFT_OUTSIDE);
    num.setNegativeFormat(num.LEFT_DASH);
    num.setNegativeRed(false);
    num.setSeparators(true, '.', ',');
    retVal = num.toFormatted();
    return retVal
}

//

function roundit(Number, NumDigitsAfterDecimal) {
    var xRet = 0
    xRet = int(0.5 + Number * (Math.pow(10, NumDigitsAfterDecimal))) / (Math.pow(10, NumDigitsAfterDecimal))
    return (xRet)
}

//				

function int(x) {
    return Math.floor(x + (x < 0 ? 1 : 0))
}

//

function isNumber(xw) {
    var anum = /(^\d+$)|(^\d+\.\d+$)/
    if (anum.test(xw))
        result = true
    else result = false;
    return (result)
}

//

function replace(txt, z, r) {
    if (z != r && z.length > 0) {
        result = txt.indexOf(z, 0);
        while (result >= 0) {
            txt = txt.substring(0, result) + r + txt.substring(result + z.length, txt.length);
            result = txt.indexOf(z, 0);
        }
    }
    return txt;
}

function isOdd(num)
{
    return num % 2;
}

//

function getSum(total, num)
{
    return total + num;
}

//-----LEAP YEAR-------------------------------------------------------------------------------------------------------------------------------------------------------------------

function leapYear(year)
{
    return ((year % 4 === 0) && (year % 100 != 0)) || (year % 400 === 0);
}

//-----DATE--------------------------------------------------------------------------------------------------------------------------------------------------------------------

function convertToDate(q)
{
    var dag = parseInt(q.substr(0, 2));
    var maand = parseInt(q.substr(3, 2)) - 1;
    var jaar = parseInt(q.substr(6, 4));
    var ret = new Date(jaar, maand, dag)

    return (ret);
}

function convertMaanden(aantal)
{
    var ret = "";
    var j = 0;

    if (aantal % 12 == 0)
    {
        ret = (aantal / 12) + " " + "jaar";
    }
    else
    {
        j = Int(aantal / 12);
        ret = j + " " + "jaar" + " " + "en" + " " + (aantal - (j * 12)) + " " + "maand(en)";
    }

    return (ret);
}

function convertMaandenToObject(aantal)
{
    var ret = { jaren: 0, maanden: 0 }
    var j = 0;

    if (aantal % 12 == 0)
    {
        ret.jaren = (aantal / 12);
        ret.maanden = 0;
    }
    else
    {
        j = int(aantal / 12);
        ret.jaren = j;
        ret.maanden = (aantal - (j * 12));
    }

    return (ret);
}

function firstDayNextMonth()
{
    var d = new Date();

    var dag = "01";
    var maand = d.getMonth()
    var jaar = d.getFullYear();

    if (maand == 11)
    {
        maand = 1
        jaar++
    }
    else
    {
        maand = maand + 2
    }

    return (dag + "/" + (maand < 10 ? "0" + maand : maand) + "/" + jaar)
}

function getToday()
{
    var d = new Date();

    var dag = d.getDate();
    var maand = d.getMonth() + 1;
    var jaar = d.getFullYear();

    return ((dag < 10 ? "0" + dag : dag) + "/" + (maand < 10 ? "0" + maand : maand) + "/" + jaar)
}

function getNextWeek()
{
    var d = new Date(new Date().getTime() + (7 * 24 * 60 * 60 * 1000));

    var dag = d.getDate();
    var maand = d.getMonth() + 1;
    var jaar = d.getFullYear();

    return (dag + "/" + (maand < 10 ? "0" + maand : maand) + "/" + jaar)
}

function checkDate(d)
{
    var wDag = 0;
    var wMaand = 0;
    var wJaar = 0;

    ret = true

    //    

    if (isNaN(d.substr(0, 2)) || isNaN(d.substr(3, 2)) || isNaN(d.substr(6, 4)))
    {
        ret = false;
    }

    //

    if (d.substr(0, 2) === "" || d.substr(3, 2) === "" || d.substr(6, 4) === "")
    {
        ret = false;
    }

    //

    if (ret === true)
    {
        wDag = parseFloat(d.substr(0, 2));
        wMaand = parseFloat(d.substr(3, 2));
        wJaar = parseFloat(d.substr(6, 4));

        // Maand

        if (wMaand === 0 || wMaand > 12)
        {
            ret = false;
        }

        // Dag

        if (wMaand === 2 && leapYear(wJaar) === true && wDag > 29)
        {
            ret = false;
        }

        if (wMaand === 2 && leapYear(wJaar) === false && wDag > 28)
        {
            ret = false;
        }

        if ((wMaand === 1 || wMaand === 3 || wMaand === 5 || wMaand === 7 || wMaand === 8 || wMaand === 10 || wMaand === 12) && wDag > 31)
        {
            ret = false;
        }

        if ((wMaand === 4 || wMaand === 6 || wMaand === 9 || wMaand === 11) && wDag > 30)
        {
            ret = false;
        }
    }

    return (ret)
}

// -----AGE---------------------------------------------------------------------------------------------------------------------------------------------------------- 

function berekenLeeftijd(dob)
{
    var year = Number(dob.substr(6, 4));
    var month = Number(dob.substr(3, 2)) - 1;
    var day = Number(dob.substr(0, 2));
    var today = new Date();
    var age = today.getFullYear() - year;
    if (today.getMonth() < month || (today.getMonth() == month && today.getDate() < day))
    {
        age--;
    }
    return (age)
}







ï»¿"use strict"

function berekenAO(xBS, xBeroepsStatuut, xInkomen, xInkomenSamenwoner, xKtl, xStopzettingYorN)
{
    var wCategorie = 0;
    var wCategorieTxt = "";

    var g1 = 990.04
    var g2 = 1593.81

    var ao_1 = 0;
    var ao_2 = 0;
    var ao_3 = 0;

    var ao_1_netto = 0;
    var ao_2_netto = 0;
    var ao_3_netto = 0;

    var ao_1_7 = 0;
    var ao_8_14 = 0;
    var ao_1_14 = 0;
    var ao_15_30 = 0;    

    var ao_1_7_netto = 0;
    var ao_8_14_netto = 0;
    var ao_1_14_netto = 0;
    var ao_15_30_netto = 0;

    var invaliditeit = 0
    var invaliditeit_netto = 0;

    var ao_jaarinkomen = 0;
    var invaliditeit_jaarinkomen = 0;

    var ao_jaarinkomen_na_pb = 0;
    var invaliditeit_jaarinkomen_na_pb = 0;

    var ao_maandinkomen_na_pb = 0;
    var invaliditeit_maandinkomen_na_pb = 0;

    var minUitkering = 0

    var bv = 0;
    var riziv = 0;
    var pb = 0;

    var ref = 0;

    var ret = {
        statuut: "",                        // 1=arbeider 2=bediende 3=ambtenaar 4=zelfstandige 5=bedrijfsleider
        statuutTxt: "",
        categorie: 0,                       // 1=met gezinslast 2=zonder gezinslast, alleenstaande 3=zonder gezinslast, samenwonende
        categorieTxt: "",
        ao_1: 0,                            // inkomen 1ste maand
        ao_1_netto: 0,
        ao_2: 0,                            // inkomen 2de maand tot en met 6de maand
        ao_2_netto: 0,
        ao_3: 0,                            // inkomen 7de maand tot en met 12 maand
        ao_3_netto: 0,
        invaliditeit: 0,                    // inkomen vanaf de 12de maand
        invaliditeit_netto: 0,
        ao_1_7: 0,                          // inkomen van dag 1 tot dag 7 - 1ste week - detail arbeiders
        ao_1_7_netto: 0,        
        ao_8_14: 0,                         // inkomen van dag 8 tot dag 14 - 2de week - detail arbeiders
        ao_8_14_netto: 0,
        ao_1_14: 0,                         // inkomen van dag 1 tot dag 14 - 1ste en 2de week - detail zelfstandigen
        ao_1_14_netto: 0,
        ao_15_30: 0,                        // inkomen van dag 15 tot dag 30 - 3de en 3de week - detail zelfstandigen
        ao_15_30_netto: 0,
        ao_jaarinkomen: 0,                  // belastbaar jaarinkomen ao
        ao_jaarinkomen_na_pb: 0,            // belastbaar jaarinkomen ao na afhouding pb aan marginaal tarief
        invaliditeit_jaarinkomen: 0,        // belastbaar jaarinkomen invaliditeit 
        invaliditeit_jaarinkomen_na_pb: 0,  // belastbaar jaarinkomen invaliditeit na afhouding pb aan marginaal tarief
        ao_maandinkomen_na_pb: 0,           // maandinkomen na pb
        invaliditeit_maandinkomen_na_pb: 0  // maandinkomen na pb
    }

    // xBS: 0=alleenstaande 1=gehuwd 2=wettelijk samenwonend
    // xBeroepsStatuut: 1=arbeider 2=bediende 3=ambtenaar 4=zelfstandige 5=bedrijfsleider
    // wCategorie: 1=met gezinslast 2=zonder gezinslast, alleenstaande 3=zonder gezinslast, samenwonende

    // 1 week = 6 dagen
    // 1 maand = 26 dagen
    // 1 jaar = 312 dagen

    var xBeroepsStatuutTxt = "";

    if (xBeroepsStatuut == 1)
    {
        xBeroepsStatuutTxt = "arbeider"
    }
    else if (xBeroepsStatuut == 2)
    {
        xBeroepsStatuutTxt = "bediende"
    }
    else if (xBeroepsStatuut == 3)
    {
        xBeroepsStatuutTxt = "ambtenaar"
    }
    else if (xBeroepsStatuut == 4)
    {
        xBeroepsStatuutTxt = "zelfstandige"
    }
    else if (xBeroepsStatuut == 5)
    {
        xBeroepsStatuutTxt = "bedrijfsleider"
    }

    //
    
    if (xBS == 0 && xKtl == 0)
    {
        wCategorie = 2
        wCategorieTxt = "alleenstaande"
    }
    else if (xBS == 0 && xKtl > 0)
    {
        wCategorie = 1;
        wCategorieTxt = "met gezinslast";
    }
    else if (xBS > 0)
    {
        if (xInkomenSamenwoner <= g1)       // partner doet de gezinslast al dan niet ontstaan  
        {
            wCategorie = 1;
            wCategorieTxt = "met gezinslast";
        }
        else if (xInkomenSamenwoner > g1 && xInkomenSamenwoner < g2)        // gelijkstelling met alleenstaande op basis van inkomsten
        {
            wCategorie = 2;
            wCategorieTxt = "alleenstaande";
        }
        else if (xInkomenSamenwoner >= g2)      // restcategorie
        {
            wCategorie = 3;
            wCategorieTxt = "samenwonende";
        }
    }

    // Maximum uitkering = 142,5279 x 60% per dag of ((142,5279 x 26) x 60% / 26)

   var  maxUitkering = 85.52 * 26;

    // 
    // 6 dagen per week, 52 weken = 26 dagen gemiddeld uit te keren per maand
    //

    if (xBeroepsStatuut == 1 || xBeroepsStatuut == 2)
    {
        // eerste maand

        if (xBeroepsStatuut == 1)
        {
            // Eerste week

            ao_1_7 = (xInkomen / 4);
            bv = ao_1_7 * 0.1111;
            ao_1_7_netto = ao_1_7 - bv;
            ref = ao_1_netto / 6;

            if (ref < 48.26)
            {
                bv = 0;
            }

            ao_1_7_netto = ao_1_7 - bv;

            // Tweede week

            ao_8_14 = (xInkomen / 4) * 0.8588;
            ao_8_14_netto = ao_8_14 - bv;
            ref = ao_2_netto / 6;

            if (ref < 48.26)
            {
                bv = 0;
            }

            ao_8_14_netto = ao_8_14 - bv;

            // Derde en vierde week

            ao_15_30 = ((xInkomen / 4)  * 0.2588) + ((xInkomen / 4) * 0.60);
            ao_15_30_netto = ao_15_30 - bv;
            ref = ao_3_netto / 6;

            if (ref < 48.26)
            {
                bv = 0;
            }

            ao_15_30_netto = ao_15_30 - bv;

            // Totaal eerste maand

            ao_1 = ao_1_7 + ao_8_14 + ao_15_30;
            bv = ao_1 * 0.1111;
            ao_1_netto = ao_1 - bv;

            ref = ao_1_netto / 26;

            if (ref < 48.26)
            {
                bv = 0;
            }

            ao_1_netto = ao_1 - bv;
        }
        else if (xBeroepsStatuut == 2)
        {
            ao_1 = xInkomen;
            bv = ao_1 * 0.1111;
            ao_1_netto = ao_1 - bv;

            ref = ao_1_netto / 26;

            if (ref < 48.26)
            {
                bv = 0;
            }

            ao_1_netto = ao_1 - bv;
        }

        // vanaf 2de maand

        ao_2 = xInkomen * 0.60;

        if (ao_2 > maxUitkering)
        {
            ao_2 = maxUitkering;
        }

        bv = ao_2 * 0.1111;
        ao_2_netto = ao_2 - bv;

        ref = ao_2_netto / 26;

        if (ref < 48.26)
        {
            bv = 0;
        }

        ao_2_netto = ao_2 - bv;

        // vanaf de zevende maand

        ao_3 = xInkomen * 0.60;

        if (wCategorie == 1)
        {
            minUitkering = 59.85 * 26;
        }
        else if (wCategorie == 2)
        {
            minUitkering = 47.89 * 26;
        }
        else if (wCategorie == 3)
        {
            minUitkering = 40.78 * 26;
        }

        if (ao_3 < minUitkering)
        {
            ao_3 = minUitkering;
        }

        if (ao_3 > maxUitkering)
        {
            ao_3 = maxUitkering;
        }

        bv = ao_3 * 0.1111;
        ao_3_netto = ao_3 - bv;

        ref = ao_3_netto / 26;

        if (wCategorie == 1)
        {
            if (ref < 48.26)
            {
                bv = 0;
            }
        }
        else
        {
            if (ref < 35.02)
            {
                bv = 0;
            }
        }

        ao_3_netto = ao_3 - bv;

        // invaliditeit vanaf 1 jaar

        if (wCategorie == 1)
        {
            invaliditeit = xInkomen * 0.65;
            minUitkering = 59.85 * 26;
            maxUitkering = 92.64 * 26;
        }
        else if (wCategorie == 2)
        {
            invaliditeit = xInkomen * 0.55;
            minUitkering = 47.89 * 26;
            maxUitkering = 78.39 * 26;
        }
        else if (wCategorie == 3)
        {
            invaliditeit = xInkomen * 0.40;
            minUitkering = 40.78 * 26;
            maxUitkering = 57.01 * 26;
        }

        //

        if (invaliditeit < minUitkering)
        {
            invaliditeit = minUitkering;
        }

        if (invaliditeit > maxUitkering)
        {
            invaliditeit = maxUitkering;
        }

        // netto invaliditeit

        riziv = invaliditeit * 0.035;
        invaliditeit_netto = invaliditeit - riziv;

        // Eventueel correctie van riziv-bijdrage

        ref = invaliditeit_netto / 26;

        if (wCategorie == 1)
        {
            if (ref < 60.88)
            {
                riziv = 0;
            }
            else if (ref >= 60.89 && ref <= 63.08)
            {
                riziv = ((ref - 60.89) * 0.035) * 26;
            }

            invaliditeit_netto = invaliditeit - riziv;
        }    
        else
        {
            if (ref < 50.54)
            {
                riziv = 0;
            }
            else if (ref >= 50.55 && ref <= 52.36)
            {
                riziv = ((ref - 50.55) * 0.035) * 26;
            }

            invaliditeit_netto = invaliditeit - riziv;
        }
    }
    else if (xBeroepsStatuut == 3)
    {

    }
    else if (xBeroepsStatuut == 4 || xBeroepsStatuut == 5)
    {
        ao_1_14 = 0;

        if (wCategorie == 1)
        {
            ao_15_30 = 59.85 * 13;
        }
        else if (wCategorie == 2)
        {
            ao_15_30 = 47.89 * 13;
        }
        else if (wCategorie == 3)
        {
            ao_15_30 = 36.47 * 13;
        }

        ao_1 = ao_15_30;
        ao_1_netto = ao_1;

        ao_1_14_netto = 0;
        ao_15_30_netto = ao_15_30;

        // Vanaf de tweede maand

        if (wCategorie == 1)
        {
            ao_2 = 59.85 * 26;
        }
        else if (wCategorie == 2)
        {
            ao_2 = 47.89 * 26;
        }
        else if (wCategorie == 3)
        {
            ao_2 = 36.47 * 26;
        }

        ao_2_netto = ao_2;

        ao_3 = ao_2;
        ao_3_netto = ao_3;

        // Invaliditeit, vanaf 1 jaar

        if (xStopzettingYorN == 0)      // ZONDER
        {
            if (wCategorie == 1)
            {
                invaliditeit = 59.85 * 26
            }
            else if (wCategorie == 2)
            {
                invaliditeit = 47.89 * 26
            }
            else if (wCategorie == 3)
            {
                invaliditeit = 36.47 * 26
            }
        }
        else if (xStopzettingYorN == 1)     // MET
        {
            if (wCategorie == 1)
            {
                invaliditeit = 59.85 * 26
            }
            else if (wCategorie == 2)
            {
                invaliditeit = 47.89 * 26
            }
            else if (wCategorie == 3)
            {
                invaliditeit = 40.70 * 26
            }
        }

        // netto invaliditeit

        riziv = invaliditeit * 0.035;
        invaliditeit_netto = invaliditeit - riziv;

        // Eventueel correctie van riziv-bijdrage

        ref = invaliditeit_netto / 26;

        if (wCategorie == 1)
        {
            if (ref < 60.88)
            {
                riziv = 0;
            }
            else if (ref >= 60.89 && ref <= 63.08)
            {
                riziv = ((ref - 60.89) * 0.035) * 26;
            }

            invaliditeit_netto = invaliditeit - riziv;
        }
        else
        {
            if (ref < 50.54)
            {
                riziv = 0;
            }
            else if (ref >= 50.55 && ref <= 52.36)
            {
                riziv = ((ref - 50.55) * 0.035) * 26;
            }

            invaliditeit_netto = invaliditeit - riziv;
        }
    }

    // Bereken PB

    ao_jaarinkomen = ao_1 + (ao_2 * 5) + (ao_3 * 6);
    pb = berekenTax(ao_jaarinkomen, 7);
    ao_jaarinkomen_na_pb = ao_jaarinkomen - pb;
    ao_maandinkomen_na_pb = ao_jaarinkomen_na_pb / 12;

    invaliditeit_jaarinkomen = invaliditeit * 12;
    pb = berekenTax(invaliditeit_jaarinkomen, 7);
    invaliditeit_jaarinkomen_na_pb = invaliditeit_jaarinkomen - pb;
    invaliditeit_maandinkomen_na_pb = invaliditeit_jaarinkomen_na_pb / 12;

    //

    ret.statuut = xBeroepsStatuut;
    ret.statuutTxt = xBeroepsStatuutTxt;
     
    ret.categorie = wCategorie;
    ret.categorieTxt = wCategorieTxt; 

    if (xBeroepsStatuut == 1)
    {
        ret.ao_1_7 = ao_1_7;
        ret.ao_1_7_netto = ao_1_7_netto;

        ret.ao_8_14 = ao_8_14;
        ret.ao_8_14_netto = ao_8_14_netto;

        ret.ao_15_30 = ao_15_30;
        ret.ao_15_30_netto = ao_15_30_netto;
    }
    else if (xBeroepsStatuut == 4 || xBeroepsStatuut == 5)
    {
        ret.ao_1_14 = ao_1_14;
        ret.ao_1_14_netto = ao_1_14_netto;

        ret.ao_15_30 = ao_15_30;
        ret.ao_15_30_netto = ao_15_30_netto;
    }
    else
    {
        ret.ao_1_7 = 0;
        ret.ao_8_14 = 0;
        ret.ao_1_14 = 0;
        ret.ao_15_30 = 0;
    }    
    
    ret.ao_1 = ao_1;
    ret.ao_1_netto = ao_1_netto;

    ret.ao_2 = ao_2;
    ret.ao_2_netto = ao_2_netto;

    ret.ao_3 = ao_3;
    ret.ao_3_netto = ao_3_netto;

    ret.invaliditeit = invaliditeit;
    ret.invaliditeit_netto = invaliditeit_netto;

    // jaarinkomen + pb

    ret.ao_jaarinkomen = ao_jaarinkomen; 
    ret.ao_jaarinkomen_na_pb = ao_jaarinkomen_na_pb;
    ret.ao_maandinkomen_na_pb = ao_maandinkomen_na_pb;

    ret.invaliditeit_jaarinkomen = invaliditeit_jaarinkomen;
    ret.invaliditeit_jaarinkomen_na_pb = invaliditeit_jaarinkomen_na_pb;
    ret.invaliditeit_maandinkomen_na_pb = invaliditeit_maandinkomen_na_pb;

    return (ret)
}

ï»¿
var PS = new Array();
var LT = new Array();
var VAPZ = new Array();

var ML = new Array();
var HW = new Array();
var MLZ = new Array();
var CO = new Array();


//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// PENSIOENSPAREN
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w1_Pension_Savings(wAge, wAgeAtBegin, wAgeAtEnd, wPv, wPmt, wRate, wRateBonus, wEntranceFee, wUseMaxPmt, wType, wTaxYorN)
{
    //  wActualAge = actual age
    //  wAgeAtBegin = age at begin of contract
    //  wAgeAtEnd = age of retirement, max = 64, min = 60
    //  wPv = actual pv(actual reserve)
    //  wPmt = yearly savings, always on 30/06 of each year
    //  wRate = rate of annual interest fixed
    //  wRateBonus = rate of annual interest variable)
    //  wEntrance fee = %
    //  wUseMaxPmt = max amount 1=use yearly max amount amount=use this amount
    //  wType = 0=bank, 1=verzekering 
    //  wTaxYorN = calculation with taxes, 1 = yes or with taxes, 0 = no or without taxes 

    var i = 0

    var wToday = new Date;
    var wYear = 0

    var wYearBegin = 0
    var wYearEnd = 0
    var wYearTax = 0

    var wPer = 0;

    var wPmtToUse = 0

    var wRateToUse = 0
    var wRateBonusToUse = 0

    var wRateTaxToUse = 0.0475;

    var wEntranceFeeToUse = 0

    var wPvToUse = 0;
    var wPvToUseTax = 0;

    var wPmtNetto = 0

    var wTaxRate = 0
    var wTaxAmount = 0

    var wInterest = 0

    var w1 = 0;
    var w2 = 0;

    var wFv = 0
    var wFvTax = 0;

    var ret = { eindkapitaal: 0, tax: 0, reserve: 0 }

    var t = "";

    //

    wRateToUse = wRate / 100
    wRateBonusToUse = wRateBonus / 100
    wEntranceFeeToUse = wEntranceFee / 100

    //

    if (wAgeAtBegin < 55 && wAge < 60)
    {
        if (wPv == 0)
        {
            // bereken een schatting van de huidige reserves tot op vandaag

            initTabelPensioensparen()

            // conversion ages to years

            wYear = wToday.getFullYear();
            wYearBegin = wYear - (wAge - wAgeAtBegin);
            wYearEnd = wYear;

            // berekening jaar 1

            if (wUseMaxPmt == 1)
            {
                wPmtToUse = getMaximumPensioensparen(wYearBegin);
            }
            else
            {
                wPmtToUse = checkMaximumPensioensparen(wYearBegin, wPmt)
            }

            wPmtNetto = wPmtToUse * (1 - wEntranceFeeToUse)
            //t += roundit(wPvToUse, 2) + "\n"

            wPvToUse = wPmtNetto;
            wPvToUseTax = wPmtToUse;

            //  fv and fv for tax purposes

            for (i = wYearBegin; i <= wYearEnd - 1; i++)
            {
                // fv

                wInterest = wPvToUse * (wRateToUse + wRateBonusToUse);
                //t += roundit(wInterest, 2) + "\n"

                wPvToUse = wPvToUse + wInterest;
                //t += roundit(wPvToUse, 2) + "\n"

                // fv tax

                wInterest = wPvToUse * wRateTaxToUse
                wPvToUseTax = wPvToUseTax + wInterest

                // volgende betaling ...

                if (wUseMaxPmt == 1)
                {
                    wPmtToUse = getMaximumPensioensparen(i + 1)
                }
                else
                {
                    wPmtToUse = checkMaximumPensioensparen(i + 1, wPmt)
                }

                wPmtNetto = wPmtToUse * (1 - wEntranceFeeToUse)
                //t += roundit(wPmtNetto, 2) + "\n"

                wPvToUse = wPvToUse + wPmtNetto;
                wPvToUseTax = wPvToUseTax + wPmtToUse;
            }

            //  correctie laatste pmt

            wPvToUse = wPvToUse - wPmtNetto
            wPvToUseTax = wPvToUseTax - wPmtToUse
        }
        else
        {
            wPvToUse = wPv;
            wPvToUseTax = wPv;
        }

        // Bewaar reserve

        ret.reserve = roundit(wPvToUse, 2);

        // fv van nu tot 60 jaar 

        if (wPmt > 1270)
        {
            wPmt = 1270;
        }

        // entrance fee

        wPmtNetto = roundit(wPmt * (1 - wEntranceFeeToUse), 2)

        // looptijd 

        wPer = 60 - wAge;

        //

        w1 = wPvToUse * Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer);
        w2 = wPmtNetto * ((Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer) - 1) / (wRateToUse + wRateBonusToUse)) * (1 + (wRateToUse + wRateBonusToUse));
        wFv = w1 + w2;

        w1 = wPvToUseTax * Math.pow(1 + wRateTaxToUse, wPer);
        w2 = wPmt * ((Math.pow(1 + wRateTaxToUse, wPer) - 1) / wRateTaxToUse) * (1 + wRateTaxToUse);
        wFvTax = w1 + w2;

        // Taks inhouden

        if (wTaxYorN == 1)
        {
            wTaxRate = 0.08;

            if (wType == 0)
            {
                wTaxAmount = wFvTax * wTaxRate;
            }
            else if (wType == 1)
            {
                wTaxAmount = wFv * wTaxRate;
            }

            wFv = wFv - wTaxAmount;
        }

        // fv van 60 jaar tot einde contract

        if (wPmt > 1270)
        {
            wPmt = 1270;
        }

        wPmtNetto = roundit(wPmt * (1 - wEntranceFeeToUse), 2)
        wPer = wAgeAtEnd - 60;

        w1 = wFv * Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer);
        w2 = wPmtNetto * ((Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer) - 1) / (wRateToUse + wRateBonusToUse)) * (1 + (wRateToUse + wRateBonusToUse));
        wFv = w1 + w2;
    }
    else if (wAgeAtBegin >= 55)
    {
        if (wPv == 0)
        {
            // bereken een schatting van de huidige reserves tot op vandaag

            initTabelPensioensparen()

            // conversion ages to years

            wYear = wToday.getFullYear();
            wYearBegin = wYear - (wAge - wAgeAtBegin);
            wYearEnd = wYear;

            // berekening jaar 1

            if (wUseMaxPmt == 1)
            {
                wPmtToUse = getMaximumPensioensparen(wYearBegin);
            }
            else
            {
                wPmtToUse = checkMaximumPensioensparen(wYearBegin, wPmt)
            }

            wPmtNetto = wPmtToUse * (1 - wEntranceFeeToUse)

            wPvToUse = wPmtNetto;
            wPvToUseTax = wPmtToUse;

            //  fv and fv for tax purposes

            for (i = wYearBegin; i <= wYearEnd - 1; i++)
            {
                // fv

                wInterest = wPvToUse * wRateToUse
                wPvToUse = wPvToUse + wInterest

                // fv tax

                wInterest = wPvToUse * wRateTaxToUse
                wPvToUseTax = wPvToUseTax + wInterest

                // volgende betaling ...

                if (wUseMaxPmt == 1)
                {
                    wPmtToUse = getMaximumPensioensparen(i + 1)
                }
                else
                {
                    wPmtToUse = checkMaximumPensioensparen(i + 1, wPmt)
                }

                wPmtNetto = wPmtToUse * (1 - wEntranceFeeToUse)

                wPvToUse = wPvToUse + wPmtNetto;
                wPvToUseTax = wPvToUseTax + wPmtToUse;
            }

            //  correctie last pmt

            wPvToUse = wPvToUse - wPmtNetto
            wPvToUseTax = wPvToUseTax - wPmtToUse
        }
        else
        {
            wPvToUse = wPv;
            wPvToUseTax = wPv;
        }

        // Bewaar reserve

        ret.reserve = roundit(wPvToUse, 2);


        // fv tot en met jaar van opname 

        if (wPmt > 1270)
        {
            wPmt = 1270;
        }

        wPmtNetto = roundit(wPmt * (1 - wEntranceFeeToUse), 2)
        wPer = wAgeAtEnd - wAge;

        //

        w1 = wPvToUse * Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer);
        w2 = wPmtNetto * ((Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer) - 1) / (wRateToUse + wRateBonusToUse)) * (1 + (wRateToUse + wRateBonusToUse));
        wFv = w1 + w2;

        w1 = wPvToUseTax * Math.pow(1 + wRateTaxToUse, wPer);
        w2 = wPmt * ((Math.pow(1 + wRateTaxToUse, wPer) - 1) / wRateTaxToUse) * (1 + wRateTaxToUse);
        wFvTax = w1 + w2;

        // Taks inhouden

        if (wTaxYorN == 1)
        {
            if (wAgeAtEnd - wAgeAtBegin < 10)
            {
                wTaxRate = 0.33
            }
            else
            {
                wTaxRate = 0.08
            }

            if (wType == 0)
            {
                wTaxAmount = wFvTax * wTaxRate;
            }
            else if (wType == 1)
            {
                wTaxAmount = wFv * wTaxRate;
            }

            wFv = wFv - wTaxAmount;
        }
    }

    ret.eindkapitaal = roundit(wFv, 2);
    ret.tax = roundit(wTaxAmount, 2);

    return (ret);
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// LANGE TERMIJNSPAREN
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w2_Longterm_Savings(wAge, wAgeAtBegin, wAgeAtEnd, wPv, wPmt, wRate, wRateBonus, wEntranceFee, wUseMaxPmt, wTaxYorN)
{
    // wActualAge = actual age
    // wAgeAtBegin = age at begin of contract
    // wAgeAtEnd = age of retirement, max = 65, min = 60
    // wPmt = yearly savings, always on 30/06 of each year
    // wRate = rate of annual interest fixed
    // wRateBonus = rate of annual interest variable)
    // wEntrance fee = %
    // wUseMaxPmt = use the maximum amount possible for each year in the past, for the future wPmt is used, values 0 = no, 1 = yes
    // wTaxYorN = calculation with taxes, 1 = yes or with taxes, 0 = no or without taxes 

    var i = 0

    var wToday = new Date;
    var wYear = 0

    var wYearBegin = 0
    var wYearEnd = 0
    var wYearTax = 0

    var wPmtToUse = 0
    var wRateToUse = 0
    var wRateBonusToUse = 0

    var wEntranceFeeToUse = 0
    var wInsuranceTax = 0

    var wPvToUse = 0;
    var wPmtNetto = 0

    var wTaxRate = 0
    var wTaxAmount = 0

    var wInterest = 0

    var wFv = 0

    var ret = { eindkapitaal: 0, tax: 0, reserve: 0 }

    var t = "";

    //

    wRateToUse = wRate / 100
    wRateBonusToUse = wRateBonus / 100
    wEntranceFeeToUse = wEntranceFee / 100
    wInsuranceTax = 0.02

    //

    if (wAgeAtBegin < 55 && wAge < 60)
    {
        if (wPv == 0)
        {
            // bereken een schatting van de huidige reserves tot op vandaag

            initTabelLangeTermijnsparen()

            // conversion ages to years

            wYear = wToday.getFullYear();
            wYearBegin = wYear - (wAge - wAgeAtBegin);
            wYearEnd = wYear;

            // berekening jaar 1

            if (wUseMaxPmt == 1)
            {
                wPmtToUse = getMaximumLangeTermijnsparen(wYearBegin);
            }
            else
            {
                wPmtToUse = checkMaximumLangeTermijnsparen(wYearBegin, wPmt)
            }

            wPmtNetto = roundit(wPmtToUse / (1 + wInsuranceTax), 2)
            wPmtNetto = wPmtNetto * (1 - wEntranceFeeToUse)
            wPvToUse = wPmtNetto;

            //  fv 

            for (i = wYearBegin; i <= wYearEnd - 1; i++)
            {
                // fv

                wInterest = wPvToUse * wRateToUse
                wPvToUse = wPvToUse + wInterest

                // volgende betaling ...

                if (wUseMaxPmt == 1)
                {
                    wPmtToUse = getMaximumLangeTermijnsparen(i + 1)
                }
                else
                {
                    wPmtToUse = checkMaximumLangeTermijnsparen(i + 1, wPmt)
                }

                wPmtNetto = roundit(wPmtToUse / (1 + wInsuranceTax), 2)
                wPmtNetto = wPmtNetto * (1 - wEntranceFeeToUse)
                wPvToUse = wPvToUse + wPmtNetto;
            }

            //  correctie last pmt

            wPvToUse = wPvToUse - wPmtNetto
        }
        else
        {
            wPvToUse = wPv;
        }

        // Bewaar reserve

        ret.reserve = roundit(wPvToUse, 2);

        // fv van nu tot 60 jaar 

        if (wPmt > 2390)
        {
            wPmt = 2390;
        }

        // entrance fee

        wPmtNetto = roundit(wPmt / (1 + wInsuranceTax), 2)
        wPmtNetto = wPmtNetto * (1 - wEntranceFeeToUse)

        wPer = 60 - wAge;

        //

        w1 = wPvToUse * Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer);
        w2 = wPmtNetto * ((Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer) - 1) / (wRateToUse + wRateBonusToUse)) * (1 + (wRateToUse + wRateBonusToUse));
        wFv = w1 + w2;

        // Taks inhouden

        if (wTaxYorN == 1)
        {
            wTaxRate = 0.08;
            wTaxAmount = wFv * wTaxRate;
            wFv = wFv - wTaxAmount;
        }

        // fv van 60 jaar tot einde contract

        if (wPmt > 2390)
        {
            wPmt = 2390;
        }

        wPmtNetto = roundit(wPmt / (1 + wInsuranceTax), 2)
        wPmtNetto = wPmtNetto * (1 - wEntranceFeeToUse)

        wPer = wAgeAtEnd - 60;

        w1 = wFv * Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer);
        w2 = wPmtNetto * ((Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer) - 1) / (wRateToUse + wRateBonusToUse)) * (1 + (wRateToUse + wRateBonusToUse));
        wFv = w1 + w2;

    }
    else if (wAgeAtBegin >= 55)
    {
        if (wPv == 0)
        {
            // bereken een schatting van de huidige reserves tot op vandaag

            initTabelLangTermijnsparen()

            // conversion ages to years

            wYear = wToday.getFullYear();
            wYearBegin = wYear - (wAge - wAgeAtBegin);
            wYearEnd = wYear;

            // berekening jaar 1

            if (wUseMaxPmt == 1)
            {
                wPmtToUse = getMaximumPensioensparen(wYearBegin);
            }
            else
            {
                wPmtToUse = checkMaximumPensioensparen(wYearBegin, wPmt)
            }

            wPmtNetto = roundit(wPmtToUse / (1 + wInsuranceTax), 2)
            wPmtNetto = wPmtNetto * (1 - wEntranceFeeToUse)

            wPvToUse = wPmtNetto;

            //  fv and fv for tax purposes

            for (i = wYearBegin; i <= wYearEnd - 1; i++)
            {
                // fv

                wInterest = wPvToUse * wRateToUse
                wPvToUse = wPvToUse + wInterest

                // volgende betaling ...

                if (wUseMaxPmt == 1)
                {
                    wPmtToUse = getMaximumPensioensparen(i + 1)
                }
                else
                {
                    wPmtToUse = checkMaximumPensioensparen(i + 1, wPmt)
                }

                wPmtNetto = roundit(wPmtToUse / (1 + wInsuranceTax), 2)
                wPmtNetto = wPmtNetto * (1 - wEntranceFeeToUse)

                wPvToUse = wPvToUse + wPmtNetto;
            }

            //  correctie last pmt

            wPvToUse = wPvToUse - wPmtNetto
        }
        else
        {
            wPvToUse = wPv;
        }

        // Bewaar reserve

        ret.reserve = roundit(wPvToUse, 2);

        // fv tot en met jaar van opname 

        if (wPmt > 2390)
        {
            wPmt = 2390;
        }

        wPmtNetto = roundit(wPmt / (1 + wInsuranceTax), 2)
        wPmtNetto = wPmtNetto * (1 - wEntranceFeeToUse)

        wPer = wAgeAtEnd - wAge;

        //

        w1 = wPvToUse * Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer);
        w2 = wPmtNetto * ((Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer) - 1) / (wRateToUse + wRateBonusToUse)) * (1 + (wRateToUse + wRateBonusToUse));
        wFv = w1 + w2;

        // Taks inhouden

        if (wTaxYorN == 1)
        {
            if (wAgeAtEnd - wAgeAtBegin < 10)
            {
                wTaxRate = 0.33
            }
            else
            {
                wTaxRate = 0.08
            }

            wTaxAmount = wFv * wTaxRate;
            wFv = wFv - wTaxAmount;
        }
    }

    ret.eindkapitaal = roundit(wFv, 2);
    ret.tax = roundit(wTaxAmount, 2);

    return (ret);}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// FV with PMT
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w3_FutureValue_Pmt(wAgeAtBegin, wAgeAtEnd, wPmt, wRate, wFrequency, wType, wEntranceFee)
{
    // wAgeAtBegin = start of placement, can be actual age or earlier
    // wAgeAtEnd = age of retirement as default
    // wPmt = periodicial savings
    // wRate = rate of annual interest (fixed + variable)
    // wFrequency = frequency of periodicial savings
    // wType = tax on interest, 0=zonder RV 1=met RV 2=type tak 21
    // wEntrance fee = if nedeed

    var wPer = 0
    var wNPer = 0
    var wPerunage = 0
    var wPmtNetto = 0
    var wFv = 0

    var wTax = 0
    var wTaxRv = 0

    var ret = { fv: 0 };

    // Taksen, RV, en instap 

    wTax = 2
    wTaxRv = 0.30

    if (wType == 2)
    {
        wPmtNetto = wPmt / (1 + (wTax / 100))
        wPmtNetto = wPmtNetto * (1 - (wEntranceFee / 100))
    }
    else
    {
        wPmtNetto = wPmt * (1 - (wEntranceFee / 100))
    }

    // Looptijd

    wPer = parseInt(wAgeAtEnd) - parseInt(wAgeAtBegin);
    wNPer = wPer * wFrequency;

    // Rentevoet

    if (wType == 0)
    {
        wPerunage = wRate / 100
    }
    else if (wType == 1)
    {
        wPerunage = (wRate * (1 - wTaxRv)) / 100
    }
    else if (wType == 2 && wPer <= 8)
    {
        wPerunage = (wRate * (1 - wTaxRv)) / 100
    }

    wPerunage = wPerunage / wFrequency

    // Berekening fv

    if (wRate == 0 && wPmt != 0)
    {
        wFv = wPmt * wNPer;
    }
    else if (wRate != 0 && wPmt != 0)
    {
        wFv = wPmtNetto * ((Math.pow(1 + wPerunage, wNPer) - 1) / wPerunage) * (1 + wPerunage)
    }
    else
    {
        wFv = 0;
    }

    ret.fv = roundit(wFv, 2);

    return (ret);
}
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// FV with PV
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w4_FutureValue_Pv(wAgeAtBegin, wAgeAtEnd, wPv, wRate, wType, wEntranceFee)
{
    // wAgeAtBegin = start of placement, can be actual age or earlier
    // wAgeAtEnd = age of retirement 
    // wPv = lump sum
    // wRate = rate of annual interest (fixed + variable)
    // wType = tax on interest, 0=zonder RV 1=met RV 2=type tak 21 
    // wEntrance fee = if nedeed

    var wPer = 0;
    var wPerunage = 0;
    var wFv = 0;
    var wPvNetto = 0;
    var wTax = 0;
    var wTaxRv = 0;

    var ret = { fv: 0 };

    // Taksen en instap 

    wTax = 2;
    wTaxRv = 0.30;

    if (wType == 2)
    {
        wPvNetto = wPv / (1 + (wTax / 100))
        wPvNetto = wPvNetto * (1 - (wEntranceFee / 100))
    }
    else
    {
        wPvNetto = wPv * (1 - (wEntranceFee / 100))
    }

    // Looptijd

    wPer = wAgeAtEnd - wAgeAtBegin

    // Rentevoet

    if (wType == 0)
    {
        wPerunage = wRate / 100
    }
    else if (wType == 1)
    {
        wPerunage = (wRate * (1 - wTaxRv)) / 100
    }
    else if (wType == 2 && wPer <= 8)
    {
        wPerunage = (wRate * (1 - wTaxRv)) / 100
    }

    // Berekening 

    if (wRate == 0)
    {
        wFv = wPv;
    }
    else
    {
        wFv = wPv * Math.pow(1 + wPerunage, wPer)
    }

    ret.fv = roundit(wFv, 2);

    return (ret)
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// GROEPSVERZEKERING
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w5_GI(wAge, wAgeAtEnd, wPv, wPmt, wRate, wRateBonus, wEntranceFee, wTaxYorN)
{
    // wActualAge = actual age
    // wAgeAtBegin = start of placement 
    // wAgeAtEnd = age of retirement, max = 65
    // wPv = actual pv (reserve)
    // wPmt = yearly future savings
    // wRate = rate of annual interest
    // wRateBonus = rate of annual interest bonus (winstdeelname)
    // wEntrance fee = if nedeed
    // wTaxYorN = tax 1 = yes, 0 = no 

    var wReserve = 0
    var wReserveTax = 0

    var wRateToUse = 0
    var wRateBonusToUse = 0
    var wRateTotal = 0

    var wEntranceFeeToUse = 0
    var wInsuranceTax = 0

    var wPer = 0
    var wPmtNetto = 0

    var wFv = 0
    var wFvTax = 0
    var wFvNetto = 0

    var w1 = 0;
    var w2 = 0;

    var wRiziv = 0;
    var wSolidariteit = 0;
    var wTaxPercentage = 0;
    var wTax = 0;

    var ret = { eindkapitaal: 0, riziv: 0, solidariteit: 0, tax: 0 } 

    // Rates to use

    wRateToUse = wRate / 100;
    wRateBonusToUse = wRateBonus / 100;
    wRateTotal = wRateToUse + wRateBonusToUse;

    wEntranceFeeToUse = wEntranceFee / 100;
    wInsuranceTax = 0.044;

    // Netto premie

    wPmtNetto = roundit(wPmt / (1 + wInsuranceTax), 2);
    wPmtNetto = roundit(wPmtNetto * (1 - wEntranceFeeToUse), 2);
    
    // Looptijd

    wPer = wAgeAtEnd - wAge;

    w1 = wPv * Math.pow(1 + wRateTotal, wPer);
    w2 = wPmtNetto * ((Math.pow(1 + wRateTotal, wPer) - 1) / wRateTotal) * (1 + wRateTotal);

    wFv = w1 + w2;

    w1 = wPv * Math.pow(1 + wRateToUse, wPer);
    w2 = wPmtNetto * ((Math.pow(1 + wRateToUse, wPer) - 1) / wRateToUse) * (1 + wRateToUse);

    wFvTax = w1 + w2;

    //

    if (wTaxYorN == 1)
    {
        // Sociale bijdragen

        wRiziv = wFv * 0.0355;
        wSolidariteit = wFv * 0.02;

        // Tax

        wTaxPercentage = getBelastingtariefStopzetting(wAgeAtEnd);
        wTaxPercentage = wTaxPercentage + (wTaxPercentage * 0.07)
        wTax = wFvTax * wTaxPercentage;

        wFvNetto = wFv - wRiziv - wSolidariteit - wTax;
    }
    else if (wTaxYorN == 0)
    {
        wFvNetto = wFv;
    }

    ret.eindkapitaal = roundit(wFvNetto, 2);
    ret.riziv = roundit(wRiziv, 2);
    ret.solidariteit = roundit(wSolidariteit, 2);
    ret.tax = roundit(wTax, 2);

    return (ret);
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// VAPZ
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w6_VAPZ(wAge, wAgeAtBegin, wAgeAtEnd, wPv, wPmt, wRate, wRateBonus, wEntranceFee, wUseMaxPmt, wType, wTaxYorN)
{
    // wActualAge = actual age
    // wAgeAtBegin = age at begin of contract
    // wAgeAtEnd = age of retirement, max = 65
    // wPv = actual pv (actual reserve)
    // wPmt = yearly future savings
    // wRate = rate of annual interest
    // wRateBonus = rate of annual interest bonus (winstdeelname)
    // wEntrance fee = if nedeed
    // wUseMaxPmt = 1=use maximum amount, or amount=use this amount 
    // wType 0=gewoon 1=sociaal
    // wTaxYorN = tax collection, 1 = yes, 0 = no 

    var wRateToUse = 0
    var wRateBonusToUse = 0
    var wEntranceFeeToUse = 0

    var wPer = 0

    var wPmtToUse = 0
    var wPmtNetto = 0

    var wInterest = 0

    var wPvToUse = 0;

    var wFv = 0
    var wFvNetto = 0

    var wRiziv = 0
    var wSolidariteit = 0

    var wToday = new Date;
    var wYear = 0

    var wYearBegin = 0
    var wYearEnd = 0

    var w1 = 0;
    var w2 = 0;

    var ret = { eindkapitaal: 0, riziv: 0, solidariteit: 0 } 

    // Rates to use

    wRateToUse = wRate / 100
    wRateBonusToUse = wRateBonus / 100
    wEntranceFeeToUse = wEntranceFee / 100

    // Calculation  

    if (wPv == 0)
    {
        // Bereken een schatting van de huidige reserve

        initTabelVapz()

        // conversion of ages to years

        wYear = wToday.getFullYear();
        wYearBegin = wYear - (wAge - wAgeAtBegin);
        wYearEnd = wYear;

        // Bereken schatting 

        if (wUseMaxPmt == 1)
        {
            wPmtToUse = getMaximumVapz(wYearBegin);
        }
        else
        {
            wPmtToUse = checkMaximumVapz(wYearBegin, wPmt);
        }

        wPmtNetto = wPmtToUse * (1 - wEntranceFeeToUse);
        wPvToUse = wPmtNetto;

        // Volgende jaren

        for (i = wYearBegin; i <= wYearEnd - 1; i++)
        {
            wInterest = wPvToUse * (wRateToUse + wRateBonusToUse);
            wPvToUse = wPvToUse + wInterest;

            if (wUseMaxPmt == 1)
            {
                wPmtToUse = getMaximumVapz(i + 1);
            }
            else
            {
                wPmtToUse = checkMaximumVapz(i + 1, wPmt);
            }

            wPmtNetto = wPmtToUse * (1 - wEntranceFeeToUse);
            wPvToUse = wPvToUse + wPmtNetto;
        }

        // Correction last pmt

        wPvToUse = wPvToUse - wPmtNetto
    }
    else if (wPv > 0)
    {
        wPvToUse = wPv;
    }

    // 
    // Eindkapitaal 
    //

    if (wType == 0 && wPmt > 3291.30)
    {
        wPmt = 3291.30;
    }
    else if (wType == 1 && wPmt > 3786.81)
    {
        wPmt = 3786.81;
    }
    
    // entrance fee

    wPmtNetto = roundit(wPmt * (1 - wEntranceFeeToUse), 2)

    // looptijd

    wPer = wAgeAtEnd - wAge

    // berekening fv

    w1 = wPvToUse * Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer);
    w2 = wPmtNetto * ((Math.pow(1 + (wRateToUse + wRateBonusToUse), wPer) - 1) / (wRateToUse + wRateBonusToUse)) * (1 + (wRateToUse + wRateBonusToUse));
    wFv = w1 + w2;

    // Tax Yes or No

    if (wTaxYorN == 1)
    {
        wRiziv = wFv * 0.0355
        wSolidariteit = wFv * 0.02

        wFvNetto = wFv - wRiziv - wSolidariteit
    }
    else if (wTaxYorN == 0)
    {
        wFvNetto = wFv
    }

    //

    ret.eindkapitaal = roundit(wFvNetto, 2);
    ret.riziv = roundit(wRiziv, 2);
    ret.solidariteit = roundit(wSolidariteit, 2);

    return (ret);
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// IPT
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w7_IPT(wAge, wAgeAtEnd, wPv, wPmt, wRate, wRateBonus, wEntranceFee, wTaxYorN)
{
    // wActualAge = actual age
    // wAgeAtBegin = age at begin of contract
    // wAgeAtEnd = age of retirement (= end of contract), max = 65
    // wPv = actual pv (reserve)
    // wPmt = yearly future savings
    // wRate = rate of annual interest
    // wRateBonus = rate of annual interest bonus (winstdeelname)
    // wEntrance fee = if nedeed
    // wTaxYorN = tax collection, 1 = yes, 0 = no 

    var wRateToUse = 0
    var wRateBonusToUse = 0
    var wRateTotal = 0;

    var wEntranceFeeToUse = 0
    var wInsuranceTax = 0

    var wPer = 0
    var wPmtNetto = 0

    var wFv = 0
    var wFvTax = 0
    var wFvNetto = 0

    var wRiziv = 0
    var wSolidariteit = 0
    var wTaxPercentage = 0
    var wTax = 0

    var w1 = 0;
    var w2 = 0;
    var wFv = 0;
    var wFvTax = 0;

    var ret = { eindkapitaal: 0, riziv: 0, solidariteit: 0, tax: 0 } 

    // Rates to use

    wRateToUse = wRate / 100
    wRateBonusToUse = wRateBonus / 100
    wRateTotal = wRateToUse + wRateBonusToUse;

    wEntranceFeeToUse = wEntranceFee / 100
    wInsuranceTax = 0.044
   
    // Netto premie

    wPmtNetto = roundit(wPmt / (1 + wInsuranceTax), 2)
    wPmtNetto = roundit(wPmtNetto * (1 - wEntranceFeeToUse), 2)

    // looptijd

    wPer = wAgeAtEnd - wAge

    // Berekening met en zonder winstdeelname

    w1 = wPv * Math.pow(1 + wRateTotal, wPer)
    w2 = wPmtNetto * ((Math.pow(1 + wRateTotal, wPer) - 1) / wRateTotal) * (1 + wRateTotal)
    wFv = w1 + w2;
       
    w1 = wPv * Math.pow(1 + wRateToUse, wPer)
    w2 = wPmtNetto * ((Math.pow(1 + wRateToUse, wPer) - 1) / wRateToUse) * (1 + wRateToUse)
    wFvTax = w1 + w2;

    // Tax Yes or No

    if (wTaxYorN == 1)
    {
        // Sociale bijdragen

        wRiziv = wFv * 0.0355;
        wSolidariteit = wFv * 0.02;

        // Tax

        wTaxPercentage = getBelastingtariefStopzetting(wAgeAtEnd);
        wTaxPercentage = wTaxPercentage + (wTaxPercentage * 0.07)
        wTax = wFvTax * wTaxPercentage;

        // Netto bedrag
    
        wFvNetto = wFv - wRiziv - wSolidariteit - wTax;
    }
    else if (wTaxYorN == 0)
    {
        wFvNetto = wFv;
    }

    ret.eindkapitaal = roundit(wFvNetto, 2);
    ret.riziv = roundit(wRiziv, 2);
    ret.solidariteit = roundit(wSolidariteit, 2);
    ret.tax = roundit(wTax, 2);

    return (ret);
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// CONVERSION PV TO PMT
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w8_Conversion_Pv(wAgeAtBegin, wAgeAtEnd, wPv, wRate, wToKeep, wTaxYorN)
{
    // wAgeAtBegin = age of retirement
    // wAgeAtEnd = age of death 
    // wPv = available funds at retirement age
    // wRate = rate of annual interest (fixed + variable)
    // wToKeep = preserved capital, a value of 0-100 is treated as a %
    // wTaxYorN = tax on interest, 0 = zonder RV, 1 = met RV 

    var wPer = 0
    var wNPer = 0
    var wPerunage = 0
    var wPmt = 0
    var wTaxRv = 0

    var wPvToKeep = 0
    var wPvToSpend = 0

    var x = 0
    var y = 0

    var ret = { pmt: 0, toKeep: 0 } 

    //

    wTaxRv = 0.30

    x = 0
    y = 0

    // Rentevoet

    if (wTaxYorN == 0)
    {
        wPerunage = wRate / 100;
    }
    else if (wTaxYorN == 1)
    {
        wPerunage = (wRate * (1 - wTaxRv)) / 100;
    }

    wPerunage = wPerunage / 12;

    // Looptijd

    wPer = wAgeAtEnd - wAgeAtBegin;
    wNPer = wPer * 12;

    // Berekening

    if (wToKeep == 100)
    {
        // Kapitaal volledig behouden

        wPmt = wPv * wPerunage;
    }
    else if (wToKeep == 0)
    {
        // Kapitaal volledig opsouperen

        wPmt = (wPv * (wPerunage / (1 - Math.pow(1 + wPerunage, -wNPer)))) * (1 / (1 + wPerunage));
    }
    else if (wToKeep > 0 && wToKeep < 100)
    {
        // Bepaald percentage behouden

        wPvToKeep = wPv * (wToKeep / 100);
        wPvToSpend = wPv - wPvToKeep;

        x = wPvToKeep * wPerunage;
        y = (wPvToSpend * (wPerunage / (1 - Math.pow(1 + wPerunage, -wNPer)))) * (1 / (1 + wPerunage));

        wPmt = x + y
    }
    else if (wToKeep > 100)
    {
        // Bepaald vast bedrag houden

        wPvToKeep = wToKeep;

        if (wToKeep > wPv)
        {
            wPvToKeep = wPv;
        }

        wPvToSpend = wPv - wPvToKeep;

        x = wPvToKeep * wPerunage;
        y = (wPvToSpend * (wPerunage / (1 - Math.pow(1 + wPerunage, -wNPer)))) * (1 / (1 + wPerunage));

        wPmt = x + y;
    }

    ret.pmt = wPmt;
    ret.toKeep = wPvToKeep;

    return (ret);
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// PENSIOENBEREKENING
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function c9_Pensioen(o)
{
    var wPensioen = 0;
    var wPensioenNetto = 0;
    var wPensioenBreuk = 0;
    var wPensioenMinimum = 0;

    var wLoonVerlies = 0;

    var wPensioenMinimumLeeftijd = 0;

    //

    var ret = { pensioenBruto: 0, pensioenNetto: 0, pensioenBreuk: 0, pensioenMinimumLeeftijd: 0 }

    //

    if (o.wGehuwdSamenwonend == 0)
    {
        wBS = 0;
    }
    else if (o.wGehuwdSamenwonend == 1)
    {
        wBS = 1;
    }

    //

    if (o.wStatuut == 1 || o.wStatuut == 2)
    {
        wPensioen = berekenPensioenWerknemer(o.wLeeftijd, o.wLeeftijdAanvang, o.wLeeftijdPensioen, o.wStatuut, o.wMaandLoon, o.wMaandLoonFuture, o.wMaandLoonCoefficient, o.wPartTime, o.wExtraInkomen, o.wGehuwdSamenwonend, o.wPartnerInkomen)
        wPensioenNetto = berekenNettoPensioen(o.wStatuut, wBS, o.wPartnerInkomen, 0, 0, 0, 0, wPensioen, 0, 0, 0)
        wPensioenMinimumLeeftijd = getMinimumPensioenLeeftijd(o.wLeeftijdAanvang)
    }
    else if (o.wStatuut == 3)
    {
        wPensioen = berekenPensioenAmbtenaar(o.wLeeftijd, o.wLeeftijdAanvang, o.wLeeftijdPensioen, o.wCategorie, o.wMaandLoon, o.wMaandLoonFuture, o.wGehuwdSamenwonend, o.wPartnerInkomen)
        wPensioenNetto = berekenNettoPensioen(o.wStatuut, wBS, o.wPartnerInkomen, 0, 0, 0, 0, wPensioen, 0, 0, 0)
        wPensioenMinimumLeeftijd = getMinimumPensioenLeeftijd(o.wLeeftijdAanvang)
    }
    else if (o.wStatuut == 4 || o.wStatuut == 5)
    {
        wPensioen = berekenPensioenZelfstandige(o.wLeeftijd, o.wLeeftijdAanvang, o.wLeeftijdPensioen, o.wMaandLoon, o.wMaandLoonFuture, o.wMaandLoonCoefficient, o.wExtraInkomen, o.wGehuwdSamenwonend, o.wPartnerInkomen)
        wPensioenNetto = berekenNettoPensioen(o.wStatuut, wBS, o.wPartnerInkomen, 0, 0, 0, 0, wPensioen, 0, 0, 0)
        wPensioenMinimumLeeftijd = getMinimumPensioenLeeftijd(o.wLeeftijdAanvang)
    }

    ret.pensioenBruto = wPensioen;
    ret.pensioenNetto = wPensioenNetto;
    ret.pensioenMinimumLeeftijd = wPensioenMinimumLeeftijd;
    
    return(ret)
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// REMEDIERING
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w10_Rescue(wAge, wAgeAtBegin, wAgeAtEnd, wGap, wRate, wToKeep, wRv)
{
    // wAge = age now, actual age
    // wAgeAtBegin = age of retirement
    // wAgeAtEnd = age of death 
    // wGap = pension gap 
    // wRate = rate of annual interest (fixed + variable)
    // wToKeep = preserved capital, a value of 0-100 is treated as a %
    // wRv = tax on interest, 0 = zonder RV, 1 = met RV 

    var wPer = 0
    var wNPer = 0
    var wPerunage = 0
    var wTaxRv = 0

    var wPv = 0
    var wPvToKeep = 0

    var wFv = 0

    var wPmt

    var wPercentageToKeep = 0
    var wPercentageToSpend = 0
    var wPercentageResult = 0

    var x = 0
    var y = 0
    var i = 0

    var wLimitOnder = 0
    var wLimitBoven = 0
    var wGuess = 0

    var ret = { amountNeeded: 0, amountToSave: 0 }

    //

    wTaxRv = 0.30
    x = 0
    y = 0

    // Rentevoet

    if (wRv == 0)
    {
        wPerunage = wRate / 100
    }
    else if (wRv == 1)
    {
        wPerunage = (wRate * (1 - wTaxRv)) / 100
    }

    wPerunage = wPerunage / 12
    
    //--------------------------------------------------------------------------------------------------------------------------------------------------
    //  PV = Funds needed for resolving the GAP
    //--------------------------------------------------------------------------------------------------------------------------------------------------

    // Looptijd

    wPer = wAgeAtEnd - wAgeAtBegin
    wNPer = wPer * 12

    // Berekening

    if (wToKeep == 100)
    {
        // Kapitaal volledig behouden

        wPv = wGap / wPerunage
    }
    else if (wToKeep == 0)
    {
        // Kapitaal volledig opsouperen

        wPv = wGap * ((1 - Math.pow(1 + wPerunage, -wNPer)) / wPerunage) * (1 + wPerunage);
    }
    else if (wToKeep > 0 && wToKeep < 100)
    {
        // Gedeelte percentage van kapitaal behouden 

        wPercentageToKeep = wToKeep / 100
        wPercentageToSpend = 1 - wPercentageToKeep

        wLimitOnder = roundit(wGap * ((1 - Math.pow(1 + wPerunage, -wNPer)) / wPerunage) * (1 + wPerunage), 2)
        wLimitBoven = roundit(wGap / wPerunage, 2)
        wGuess = wLimitBoven - ((wLimitBoven - wLimitOnder) / 2)

        for (i = 0; i <= 50; i++)
        {
            x = wGap * ((Math.pow(1 + wPerunage, wNPer) - 1) / wPerunage) * (1 + wPerunage)
            y = wGuess * (Math.pow(1 + wPerunage, wNPer));

            wFv = y - x

            wPercentageResult = wFv / wGuess

            if (wPercentageResult < wPercentageToKeep)
            {
                wLimitOnder = wGuess
            }                
            else if (wPercentageResult >= wPercentageToKeep)
            {
                wLimitBoven = wGuess
            }

            wGuess = wLimitBoven - ((wLimitBoven - wLimitOnder) / 2)
        }

        wPv = roundit(wGuess, 2)
        wPvToKeep = roundit(wFv, 2)
    }

    //--------------------------------------------------------------------------------------------------------------------------------------------------
    //  PMT = Monthly savings needed to reach the PV amount
    //--------------------------------------------------------------------------------------------------------------------------------------------------

    // Looptijd

    wPer = wAgeAtBegin - wAge
    wNPer = wPer * 12

    // Berekening

    wPmt = (wPv / ((Math.pow(1 + wPerunage, wNPer) - 1) / wPerunage)) * (1 / (1 + wPerunage));

    //

    ret.amountNeeded = roundit(wPv, 2)
    ret.amountToSave = roundit(wPmt, 2)

    return(ret);
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// PENSIOENSPAREN BEREKENING VGL + RENDEMENT
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w11_Pension_Savings_Yield(wAgeAtBegin, wAgeAtEnd, wPmt, wRate, wEntranceFee)
{
    // wAgeAtBegin = age at begin of the savings
    // wAgeAtEnd = age of pension 
    // wPmt = Yearly savings amount
    // wRate = rate of annual interest (fixed + variable)
    // wEntranceFee = if needed

    var wGrens_1 = 990;
    var wGrens_2 = 1270;

    var wTaxVoordeel_1 = 0.30;
    var wTaxVoordeel_2 = 0.25;

    var wTaxVoordeelProcent = 0;
    var wTaxVoordeel = 0;
    var wTaxVoordeelSum = 0;

    var wPmtNetto = 0;
    var wPmtNettoSum = 0;

    var wPmtToUse = 0;

    var wPer = 0;
    var wPer60 = 0;
    var wPer65 = 0;

    var wFv60 = 0;
    var wFv60Netto = 0;

    var wTaxLTSProcent = 0;
    var wTaxLTS = 0;

    var wPer

    var wFv = 0;
    var wFvNetto = 0;

    var wRendement = 0;

    var w1 = 0;
    var w2 = 0;

    var tRendement = [];

    var ret = { wAgeAtBegin: 0, wAgeAtEnd: 0, wPmt: 0, wTaxVoordeelProcent: 0, wTaxVoordeel: 0, wPmtNetto: 0, wPer: 0, wPmtNettoSum: 0, wTaxVoordeelSum: 0, wRate: 0, wEntranceFee: 0, wFv60: 0, wTaxLTS: 0, wFv60Netto: 0, wFv: 0, wRendement: 0 };

    // Percentage belastingvoordeel

    wPmt = (wPmt > wGrens_2 ? wGrens_2 : wPmt);

    if (wPmt <= wGrens_1)
    {
        wTaxVoordeelProcent = wTaxVoordeel_1;
    }
    else if (wPmt > wGrens_1)
    {
        wTaxVoordeelProcent = wTaxVoordeel_2;
    }

    // Berekeningen

    if (wAgeAtBegin < 55)
    {
        wTaxVoordeel = wPmt * wTaxVoordeelProcent;                                                                      // Procent belastingvoordeel
        wPmtNetto = wPmt - wTaxVoordeel;                                                                                // Netto te betalen per jaar na aftrek belastingvoordeel
        wPer = wAgeAtEnd - wAgeAtBegin;                                                                                 // Aantal stortingen

        wPmtNettoSum = wPmtNetto * wPer;                                                                                // Totaalbedrag van de netto stortingen
        wTaxVoordeelSum = wTaxVoordeel * wPer;                                                                          // Totaalbedrag belastingvoordeel

        wPer60 = 60 - wAgeAtBegin;                                                                                      // Aantal stortingen tot 60 jaar;
        wPmtToUse = wPmt * (1 - (wEntranceFee / 100))                                                                   // Jaarlijkse storting na aftrek van de instapkosten;

        wFv60 = wPmtToUse * ((Math.pow(1 + (wRate / 100), wPer60) - 1) / (wRate / 100)) * (1 + (wRate / 100));          // Totaal kapitaal op 60 jaar
        wTaxLTSProcent = 0.08;                                                                                          // Tax LTS procent;
        wTaxLTS = wFv60 * wTaxLTSProcent;                                                                               // Tax LTS bedrag
        wFv60Netto = wFv60 - wTaxLTS;                                                                                   // Totaal kapitaal op 60 jaar na aftrek TaksLTS

        wPer65 = wAgeAtEnd - 60;                                                                                        // Looptijd van 60 jaar tot einde

        w1 = wFv60Netto * Math.pow(1 + (wRate / 100), wPer65);
        w2 = wPmtToUse * ((Math.pow(1 + (wRate / 100), wPer65) - 1) / (wRate / 100)) * (1 + (wRate / 100));
        wFv = w1 + w2;                                                                                                  // Netto pensioenkapitaal

        // Berekening rendement

        // Van beginjaar tot 60 jaar;

        for (i = 1; i <= wPer60; i++)
        {
            tRendement.push(wPmtNetto * -1);
        }

        tRendement.push((wTaxLTS + wPmtNetto) * -1);

        for (i = 1; i <= wPer65 - 1; i++)
        {
            tRendement.push(wPmtNetto * -1);
        }

        tRendement.push(wFv);
        //printTabel(tRendement, wAgeAtBegin)

        wRendement = irr(tRendement);
    }
    else if (wAgeAtBegin >= 55)
    {
        wTaxVoordeel = wPmt * wTaxVoordeelProcent;                                                                      // Procent belastingvoordeel
        wPmtNetto = wPmt - wTaxVoordeel;                                                                                // Netto te betalen per jaar na aftrek belastingvoordeel
        wPer = wAgeAtEnd - wAgeAtBegin;                                                                                 // Aantal stortingen

        wPmtNettoSum = wPmtNetto * wPer;                                                                                // Totaalbedrag van de netto stortingen
        wTaxVoordeelSum = wTaxVoordeel * wPer;                                                                          // Totaalbedrag belastingvoordeel

        wPmtToUse = wPmt * (1 - (wEntranceFee / 100))                                                                   // Jaarlijkse storting na aftrek van de instapkosten;

        wFv = wPmtToUse * ((Math.pow(1 + (wRate / 100), wPer) - 1) / (wRate / 100)) * (1 + (wRate / 100));              // Totaal kapitaal op 60 jaar

        if (wPer < 10)
        {
            wTaxLTSProcent = 0.10;                                                                                      // Tax LTS procent;
        }
        else if (wPer >= 10)
        {
            wTaxLTSProcent = 0.08;                                                                                      // Tax LTS procent;
        }
        
        wTaxLTS = wFv * wTaxLTSProcent;                                                                                 // Tax LTS bedrag
        wFvNetto = wFv - wTaxLTS;                                                                                       // Totaal kapitaal op 60 jaar na aftrek TaksLTS

        // Berekening rendement

        for (i = 1; i <= wPer; i++)
        {
            tRendement.push(wPmtNetto * -1);
        }

        tRendement.push(wFv - wTaxLTS);
        //printTabel(tRendement, wAgeAtBegin)

        wRendement = irr(tRendement);
    }

    // Return object

    ret.wAgeAtBegin = wAgeAtBegin;
    ret.wAgeAtEnd = wAgeAtEnd;

    ret.wPmt = wPmt;

    ret.wTaxVoordeelProcent = wTaxVoordeelProcent * 100;
    ret.wTaxVoordeel = wTaxVoordeel;
    ret.wPmtNetto = wPmtNetto;

    ret.wPer = wPer;

    ret.wPmtNettoSum = wPmtNettoSum;
    ret.wTaxVoordeelSum = wTaxVoordeelSum;

    ret.wRate = wRate;
    ret.wEntranceFee = wEntranceFee;

    if (wAgeAtBegin < 55)
    {
        ret.wFv60 = wFv60;
        ret.wTaxLTS = wTaxLTS;
        ret.wFv60Netto = wFv60Netto;

        ret.wFv = wFv;
    }
    else if (wAgeAtBegin >= 55)
    {
        ret.wFv60 = 0;
        ret.wTaxLTS = 0;
        ret.wFv60Netto = 0;

        ret.wFv = wFvNetto;
    }
    
    ret.wRendement = wRendement;

    return (ret);
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// AFKOOP STUDIEJAREN
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w12_Afkoop_Studiejaren(oAantalJaren, oPensioen, oPensioenNaarNetto)
{
    // oAantalJaren = aantal jaren
    // oPensioen = object met gegevens pensioenberekeneing
    // oPensioenNaarNetto = object met gegevens voor omrekenen bruto-pensioen naar netto-pensioen

    var p; 

    var ret = { wExtraPensioen: 0, wNettoBefore: 0, wNettoAfter: 0 };

    var pensioen = 0;
    var pensioenExtra = 0;

    var brutoPensioenBefore = 0;
    var nettoPensioenBefore = 0;

    var brutoPensioenAfter = 0;
    var nettoPensioenAfter = 0;

    var wBS = 0;

    //

    if (oPensioenNaarNetto.wGehuwdSamenwonend == 0)
    {
        wBS = 0;
    }
    else if (oPensioenNaarNetto.wGehuwdSamenwonend == 1)
    {
        wBS = 1;
    }

    // Bereken netto huidig pensioenbedrag

    p = c9_Pensioen(oPensioen);

    brutoPensioenBefore = p.pensioenBruto;
    nettoPensioenBefore = p.pensioenNetto;

    // Pensioenbedrag met extra bedrag van de afkoop

    if (oPensioen.wGehuwdSamenwonend == 0)
    {
        brutoPensioenAfter = brutoPensioenBefore + ((266.67 * oAantalJaren) / 12);
    }
    else if (oPensioen.wGehuwdSamenwonend == 1 && oPensioen.wPartnerInkomen == 1)
    {
        brutoPensioenAfter = brutoPensioenBefore + ((266.67 * oAantalJaren) / 12);
    }
    else if (oPensioen.wGehuwdSamenwonend == 1 && oPensioen.wPartnerInkomen == 0)
    {
        brutoPensioenAfter = brutoPensioenBefore + ((333.33 * oAantalJaren) / 12);
    }

    // Bereken netto pensioen met extra bedrag van de afkoop

    nettoPensioenAfter = berekenNettoPensioen(oPensioenNaarNetto.wStatuut, wBS, oPensioenNaarNetto.wPartnerInkomen, 0, 0, 0, 0, brutoPensioenAfter, 0, 0, 0);
    pensioenExtra = nettoPensioenAfter - nettoPensioenBefore;

    // Resultaat

    ret.wExtraPensioen = pensioenExtra;
    ret.wNettoBefore = nettoPensioenBefore;
    ret.wNettoAfter = nettoPensioenAfter;

    return (ret);
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// POZ
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w13_POZ(wAge, wAgeAtEnd, wPv, wPmt, wRate, wRateBonus, wEntranceFee, wTaxYorN)
{
    // wActualAge = actual age
    // wAgeAtBegin = start of placement 
    // wAgeAtEnd = age of retirement, max = 65
    // wPv = actual pv (reserve)
    // wPmt = yearly future savings
    // wRate = rate of annual interest
    // wRateBonus = rate of annual interest bonus (winstdeelname)
    // wEntrance fee = if nedeed
    // wTaxYorN = tax 1 = yes, 0 = no 

    var wReserve = 0
    var wReserveTax = 0

    var wRateToUse = 0
    var wRateBonusToUse = 0
    var wRateTotal = 0

    var wEntranceFeeToUse = 0
    var wInsuranceTax = 0

    var wPer = 0
    var wPmtNetto = 0

    var wFv = 0
    var wFvTax = 0
    var wFvNetto = 0

    var w1 = 0;
    var w2 = 0;

    var wRiziv = 0;
    var wSolidariteit = 0;
    var wTaxPercentage = 0;
    var wTax = 0;

    var ret = { eindkapitaal: 0, riziv: 0, solidariteit: 0, tax: 0 }

    // Rates to use

    wRateToUse = wRate / 100;
    wRateBonusToUse = wRateBonus / 100;
    wRateTotal = wRateToUse + wRateBonusToUse;

    wEntranceFeeToUse = wEntranceFee / 100;
    wInsuranceTax = 0.044;

    // Netto premie

    wPmtNetto = roundit(wPmt / (1 + wInsuranceTax), 2);
    wPmtNetto = roundit(wPmtNetto * (1 - wEntranceFeeToUse), 2);

    // Looptijd

    wPer = wAgeAtEnd - wAge;

    w1 = wPv * Math.pow(1 + wRateTotal, wPer);
    w2 = wPmtNetto * ((Math.pow(1 + wRateTotal, wPer) - 1) / wRateTotal) * (1 + wRateTotal);

    wFv = w1 + w2;

    w1 = wPv * Math.pow(1 + wRateToUse, wPer);
    w2 = wPmtNetto * ((Math.pow(1 + wRateToUse, wPer) - 1) / wRateToUse) * (1 + wRateToUse);

    wFvTax = w1 + w2;

    //

    if (wTaxYorN == 1)
    {
        // Sociale bijdragen

        wRiziv = wFv * 0.0355;
        wSolidariteit = wFv * 0.02;

        // Tax

        wTaxPercentage = getBelastingtariefStopzetting(wAgeAtEnd);
        wTaxPercentage = wTaxPercentage + (wTaxPercentage * 0.07)
        wTax = wFvTax * wTaxPercentage;

        wFvNetto = wFv - wRiziv - wSolidariteit - wTax;
    }
    else if (wTaxYorN == 0)
    {
        wFvNetto = wFv;
    }

    ret.eindkapitaal = roundit(wFvNetto, 2);
    ret.riziv = roundit(wRiziv, 2);
    ret.solidariteit = roundit(wSolidariteit, 2);
    ret.tax = roundit(wTax, 2);

    return (ret);
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// VAPW
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function w14_VAPW(wAge, wAgeAtEnd, wPv, wPmt, wRate, wRateBonus, wEntranceFee, wTaxYorN, wMaandloon, wMaandloonCoeff, wPmtCorrectie)
{
    // wActualAge = actual age
    // wAgeAtBegin = start of placement 
    // wAgeAtEnd = age of retirement, max = 65
    // wPv = actual pv (reserve)
    // wPmt = yearly future savings
    // wRate = rate of annual interest
    // wRateBonus = rate of annual interest bonus (winstdeelname)
    // wEntrance fee = if nedeed
    // wTaxYorN = tax 1 = yes, 0 = no 
    // wMaandloon = bruto maandloon
    // wMaandloonCoeff = bruto maandloon
    // wPmtCorrectie = correctie premie via andere stortingen, indien nog niet verrekend in wPmt

    var wReserve = 0
    var wReserveTax = 0

    var wRateToUse = 0
    var wRateBonusToUse = 0
    var wRateTotal = 0

    var wEntranceFeeToUse = 0
    var wInsuranceTax = 0

    var wPer = 0;
    var wPmtToUse = 0;
    var wPmtNetto = 0;

    var wPmtMax = 0;

    var wFv = 0
    var wFvTax = 0
    var wFvNetto = 0

    var w1 = 0;
    var w2 = 0;

    var wRiziv = 0;
    var wSolidariteit = 0;
    var wTaxPercentage = 0;
    var wTax = 0;

    var ret = { pmt: 0, pmtNetto: 0, eindkapitaal: 0, riziv: 0, solidariteit: 0, tax: 0 }

    // Controle van de premie

    wPmtMax = (wMaandloon * wMaandloonCoeff) * 0.03; 

    if (wMaandloon * wMaandloonCoeff < 54000)
    {
        wPmtMin = 1620;
    }

    // Rates to use

    wRateToUse = wRate / 100;
    wRateBonusToUse = wRateBonus / 100;
    wRateTotal = wRateToUse + wRateBonusToUse;

    wEntranceFeeToUse = wEntranceFee / 100;
    wInsuranceTax = 0.044;

    // Netto premie te gebruiken

    wPmtToUse = (wPmt > wPmtMax ? wPmtMax : wPmt);
    wPmtToUse = wPmtToUse - wPmtCorrectie;
    wPmtToUse = (wPmtToUse < 0 ? 0 : wPmtToUse);

    wPmtNetto = roundit(wPmtToUse / (1 + wInsuranceTax), 2);
    wPmtNetto = roundit(wPmtNetto * (1 - wEntranceFeeToUse), 2);

    // Looptijd

    wPer = wAgeAtEnd - wAge;

    w1 = wPv * Math.pow(1 + wRateTotal, wPer);
    w2 = wPmtNetto * ((Math.pow(1 + wRateTotal, wPer) - 1) / wRateTotal) * (1 + wRateTotal);

    wFv = w1 + w2;

    //

    if (wTaxYorN == 1)
    {
        // Sociale bijdragen

        wRiziv = wFv * 0.0355;
        wSolidariteit = wFv * 0.02;

        // Tax

        wTaxPercentage = 0.10;
        wTax = wFv * wTaxPercentage;

        wFvNetto = wFv - wRiziv - wSolidariteit - wTax;
    } 
    else if (wTaxYorN == 0)
    {
        wFvNetto = wFv;
    }

    ret.pmt = roundit(wPmtToUse, 2);
    ret.pmtNetto = roundit(wPmtNetto, 2);
    ret.eindkapitaal = roundit(wFvNetto, 2);
    ret.riziv = roundit(wRiziv, 2);
    ret.solidariteit = roundit(wSolidariteit, 2);
    ret.tax = roundit(wTax, 2);

    return (ret);
}


//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// HULPFUNCTIES
//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function getBelastingtariefStopzetting(wAgeAtEnd)
{
    var wTaxPercentage = 0;

    if (wAgeAtEnd == 60)
    {
        wTaxPercentage = 0.2019
    }
    else if (wAgeAtEnd == 61)
    {
        wTaxPercentage = 0.1817
    }
    else if (wAgeAtEnd >= 62 && wAgeAtEnd <= 64)
    {
        wTaxPercentage = 0.1666
    }
    else if (wAgeAtEnd >= 65)
    {
        wTaxPercentage = 0.1009
    }

    return (wTaxPercentage)
}

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

function getMaximumPensioensparen(jaar)
{
    var txt = "";
    var ret = 0;
    var j = 0;

    ret = 127000;

    if (jaar < 1986)
    {
        ret = 0
    }
    else
    {
        for (j = 1; j <= PS.length - 1; j++)
        {
            txt = PS[j];

            if (txt.substr(0, 4) == jaar)
            {
                ret = parseFloat(txt.substr(5))
            }
        }
    }

    return (ret / 100)
}

function checkMaximumPensioensparen(jaar, m)
{
    var txt = "";
    var ret = 0;
    var j = 0;

    // Maximumbedrag

    var max = 127000;

    if (jaar < 1987)
    {
        max = 0;
    }
    else
    {
        for (j = 1; j <= PS.length - 1; j++)
        {
            txt = PS[j];

            if (txt.substr(0, 4) == jaar)
            {
                max = parseFloat(txt.substr(5));
            }
        }
    }

    // Check

    if (max != 0)
    {
        ret = Math.min(m, (max / 100));
    }

    return (ret)
}

