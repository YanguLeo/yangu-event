function berekenKindergeld(gewest, kinderen)
{
    // gewest 0=vlaanderen 1=brussel 2=wallonie 3=duitstalige gemeenschap

    // structuur van kinderen = { leeftijd, zorg, weeskind, halfweeskind, pleegkind, inkomen, eenoudergezin, opvangdagen, ouderinvalide }

    // leeftijd
    // zorg: categorie 0=geen zorg van 1 tot en met 9 zoals opgesomd in de betreffende lijsten. Indien niet gekend en zorgbehoevend dan kode 1 gebruiken.
    // weeskind: 0=nee 1=ja
    // halfweeskind: 0=nee 1=ja
    // pleegkind: 0=nee 1=ja
    // inkomen: beroepsinkomsten + onderhoudsgelden + onroeren goederen + vervangingsinkomsten + leefloon  WN=bruto beroepsinkomsten ZELF=netto beroepsinkomsten Verschilt per gewest: Vlaanderen is ZELF * factor 100/80
    // eenoudergezin: 0=nee 1=ja
    // opvangdagen: getal, aantal dagen in kinderopvang, enkel voor vlaanderen
    // ouderinvalide: 0=nee 1=ja een of beide ouders zijn invalide;

    var d = new Date();

    //

    var basis = 0;
    var leeftijdstoeslag = 0;
    var wezentoeslag = 0;
    var pleegzorgtoeslag = 0;
    var zorgtoeslag = 0;
    var socialetoeslag = 0;
    var schoolbonus = 0;
    var kinderopvangtoeslag = 0;
    var kleutertoeslag = 0;
    var eenoudertoeslag = 0;
    var schooltoeslag = 0;

    var geboortepremie = 0;

    //

    var kindergeld = 0;
    var detail = [];

    //

    var jaar = 0;
    var geboortejaar = 0;

    //

    var i = 0;
    var k = 0;

    //

    var ret = { kindergeld: 0, detail: [] };

    //

    var o = kinderen.sortBy('leeftijd');

    //

    jaar = parseInt(d.getFullYear());
    

    //

    if (gewest == 0)
    {
        for (i = 0; i <= o.length - 1; i++)
        {
            basis = 0;
            leeftijdstoeslag = 0;
            wezentoeslag = 0;
            pleegzorgtoeslag = 0;
            zorgtoeslag = 0;
            socialetoeslag = 0;
            schoolbonus = 0;
            kinderopvangtoeslag = 0;
            kleutertoeslag = 0;
            eenoudertoeslag = 0;
            schooltoeslag = 0;
            geboortepremie = 0;

            //
    
            geboortejaar = jaar - o[i].leeftijd;

            //

            if (geboortejaar >= 2019)
            {
                // geboortepremie eenmalig

                geboortepremie = 1122.00;

                //

                basis = 163.20;

                // Wezentoeslag

                if (o[i].weeskind == 1)
                {
                    wezentoeslag = 163.20;
                }
                else if (o[i].halfweeskind == 1)
                {
                    wezentoeslag = 81.60;
                }

                // Zorgtoeslag

                if (o[i].zorg == 1)
                {
                    zorgtoeslag = 82.37
                }
                else if (o[i].zorg == 2)
                {
                    zorgtoeslag = 109.70
                }
                else if (o[i].zorg == 3)
                {
                    zorgtoeslag = 422.56
                }
                else if (o[i].zorg == 4)
                {
                    zorgtoeslag = 255.99
                }
                else if (o[i].zorg == 5)
                {
                    zorgtoeslag = 422.56
                }
                else if (o[i].zorg == 6)
                {
                    zorgtoeslag = 422.56
                }
                else if (o[i].zorg == 7)
                {
                    zorgtoeslag = 480.48
                }
                else if (o[i].zorg == 8)
                {
                    zorgtoeslag = 514.80
                }
                else if (o[i].zorg == 9)
                {
                    zorgtoeslag = 549.12
                }

                // Pleegzorgtoeslag

                if (o[i].pleegkind == 1)
                {
                    pleegzorgtoeslag = 63.03;
                }

                // Sociale toeslag

                grensinkomen_1 = 30984.00;
                grensinkomen_2 = 61200.00;

                if (kinderen.length <= 2 && o[i].inkomen > 0 && o[i].inkomen < grensinkomen_1)
                {
                    socialetoeslag = 51.00;
                }
                else if (kinderen.length > 2 && o[i].inkomen > 0 && o[i].inkomen < grensinkomen_1)
                {
                    socialetoeslag = 81.60;
                }
                else if (kinderen.length > 2 && o[i].inkomen > 0 && o[i].inkomen >= grensinkomen_1 && inkmen < grensinkomen_2)
                {
                    socialetoeslag = 61.20;
                }

                // Schoolbonus eenmalig

                if (o[i].leeftijd >= 0 && o[i].leeftijd <= 4)
                {
                    schoolbonus = 20.40;
                }
                else if (o[i].leeftijd >= 5 && o[i].leeftijd <= 11)
                {
                    schoolbonus = 35.70;
                }
                else if (o[i].leeftijd >= 12 && o[i].leeftijd <= 17)
                {
                    schoolbonus = 51.00;
                }
                else if (o[i].leeftijd >= 18 && o[i].leeftijd <= 24)
                {
                    schoolbonus = 61.20;
                }

                // Kinderopvangtoeslag per opvangdag

                kinderopvangtoeslag = o[i].opvangdagen * 3.23;

                // Kleutertoeslag eenmalig

                if (o[i].leeftijd >= 0 && o[i].leeftijd <= 4)
                {
                    kleutertoeslag = 132.60;
                }

                // Schooltoeslag eenmalig

                if (o[i].leeftijd >= 3 && o[i].leeftijd <= 5)
                {
                    schooltoeslag = 103.70;
                }
                else if (o[i].leeftijd > 5 && o[i].leeftijd <= 12)
                {
                    schooltoeslag = 194.00;
                }
                else if (o[i].leeftijd > 12 && o[i].leeftijd <= 18)
                {
                    schooltoeslag = 630.00;
                }
                else if (o[i].leeftijd > 18 && o[i].leeftijd <= 24)
                {
                    schooltoeslag = 50.00;
                }

                // kindergeld 

                k = basis + wezentoeslag + zorgtoeslag + pleegzorgtoeslag + socialetoeslag + kinderopvangtoeslag;

                kindergeld += k;
                detail.push({ geboortejaar: geboortejaar, kindergeld: k, geboortepremie: geboortepremie, schoolbonus: schoolbonus, schooltoeslag: schooltoeslag, kleutertoeslag: kleutertoeslag });

            }
            else if (geboortejaar < 2019)
            {
                if (o[i].weeskind == 1 || o[i].halfweeskind == 1)
                {
                    basis = 360.83;
                }
                else
                {
                    if (i == 0)
                    {
                        basis = 93.93;
                    }
                    if (i == 1)
                    {
                        basis = 173.80;
                    }
                    if (i >= 2)
                    {
                        basis = 259.49;
                    }
                }

                // Leeftijdstoeslag

                if (o[i].leeftijd >= 6 && o[i].leeftijd <= 11)
                {
                    leeftijdstoeslag = 32.63;
                }
                else if (o[i].leeftijd >= 12 && o[i].leeftijd <= 17)
                {
                    leeftijdstoeslag = 49.86;
                }
                else if (o[i].leeftijd >= 18 && o[i].leeftijd <= 24)
                {
                    leeftijdstoeslag = 63.40;
                }

                // Zorgtoeslag

                if (o[i].zorg == 1)
                {
                    zorgtoeslag = 82.37
                }
                else if (o[i].zorg == 2)
                {
                    zorgtoeslag = 109.70
                }
                else if (o[i].zorg == 3)
                {
                    zorgtoeslag = 422.56
                }
                else if (o[i].zorg == 4)
                {
                    zorgtoeslag = 255.99
                }
                else if (o[i].zorg == 5)
                {
                    zorgtoeslag = 422.56
                }
                else if (o[i].zorg == 6)
                {
                    zorgtoeslag = 422.56
                }
                else if (o[i].zorg == 7)
                {
                    zorgtoeslag = 480.48
                }
                else if (o[i].zorg == 8)
                {
                    zorgtoeslag = 514.80
                }
                else if (o[i].zorg == 9)
                {
                    zorgtoeslag = 549.12
                }

                // Pleegzorgtoeslag

                if (o[i].pleegkind == 1)
                {
                    pleegzorgtoeslag = 63.03;
                }

                // Sociale toeslag

                socialetoeslag = 0;

                // Toeslag eenoudergezin

                if (o[i].eenoudergezin == 1 && i == 0)
                {
                    eenoudertoeslag = 47.81;
                }
                else if (o[i].eenoudergezin == 1 && i == 1)
                {
                    eenoudertoeslag = 29.64;
                }
                else if (o[i].eenoudergezin == 1 && i >= 2)
                {
                    eenoudertoeslag = 23.90;
                }

                // Schoolbonus eenmalig

                if (o[i].leeftijd >= 0 && o[i].leeftijd <= 4)
                {
                    schoolbonus = 20.40;
                }
                else if (o[i].leeftijd >= 5 && o[i].leeftijd <= 11)
                {
                    schoolbonus = 35.70;
                }
                else if (o[i].leeftijd >= 12 && o[i].leeftijd <= 17)
                {
                    schoolbonus = 51.00;
                }
                else if (o[i].leeftijd >= 18 && o[i].leeftijd <= 24)
                {
                    schoolbonus = 61.20;
                }

                // Kinderopvangtoeslag per opvangdag

                kinderopvangtoeslag = o[i].opvangdagen * 3.23;

                // Kleutertoeslag eenmalig

                if (o[i].leeftijd >= 0 && o[i].leeftijd <= 4)
                {
                    kleutertoeslag = 132.60;
                }

                // Schooltoeslag eenmalig

                if (o[i].leeftijd >= 3 && o[i].leeftijd <= 5)
                {
                    schooltoeslag = 103.70;
                }
                else if (o[i].leeftijd > 5 && o[i].leeftijd <= 12)
                {
                    schooltoeslag = 194.00;
                }
                else if (o[i].leeftijd > 12 && o[i].leeftijd <= 18)
                {
                    schooltoeslag = 630.00;
                }
                else if (o[i].leeftijd > 18 && o[i].leeftijd <= 24)
                {
                    schooltoeslag = 50.00;
                }

                // kindergeld 

                k = basis + leeftijdstoeslag + wezentoeslag + zorgtoeslag + pleegzorgtoeslag + socialetoeslag + eenoudertoeslag + kinderopvangtoeslag;

                kindergeld += k;
                detail.push({ geboortejaar: geboortejaar, kindergeld: k, geboortepremie: 0, schoolbonus: schoolbonus, schooltoeslag: schooltoeslag, kleutertoeslag: kleutertoeslag });
            }
        }
    }
    else if (gewest == 1)
    {
        for (i = 0; i <= o.length - 1; i++)
        {
            basis = 0;
            leeftijdstoeslag = 0;
            wezentoeslag = 0;
            pleegzorgtoeslag = 0;
            zorgtoeslag = 0;
            socialetoeslag = 0;
            schoolbonus = 0;
            kinderopvangtoeslag = 0;
            kleutertoeslag = 0;
            eenoudertoeslag = 0;
            schooltoeslag = 0;
            geboortepremie = 0;

            //

            geboortejaar = jaar - o[i].leeftijd;

            //

            if (geboortejaar >= 2020)
            {

            }
            else if (geboortejaar < 2020)
            {
                if (o[i].weeskind == 1 || o[i].halfweeskind == 1)
                {
                    basis = 368.03;
                }
                else if (o[i].pleegkind == 1)
                {
                    basis = 64.28;
