/**
 * Created by didokun on 04/07/16.
 */

/*Age validation*/
jQuery.validator.addMethod('age', function (age, element, param) {
    // if date format in french
    value = age.split("/");
    for (i = 0; i < 3; i++) {
        value[i] = parseInt(value[i]);
    }
    // years, month -1 (month in JS start with 0), day
    age = new Date(value[2], value[1] -1,  value[0]);
    var ageDifMs = Date.now() - age.getTime();
    var ageDate = new Date(ageDifMs);
    ageDate =  Math.abs(ageDate.getUTCFullYear() - 1970);
    console.log('AAAAGE ' + ageDate);
    return this.optional(element) || ageDate >= param;
}, Drupal.t('Enter a valid age'));


/*Validate File file size*/
jQuery.validator.addMethod('filesize', function(value, element, param) {
    console.log("SIZE "+element.files[0].size);
    return this.optional(element) || (element.files[0].size <= 1048576*param)
}, "Large size file");

/*Check File extension*/
jQuery.validator.addMethod('extension', function(value, element, param) {
    var fileExtension = param.split("|");
    return this.optional(element) || jQuery.inArray(jQuery(element).val().split('.').pop().toLowerCase(), fileExtension) != -1;
}, "Extension not required");

/*Mail Validation*/
jQuery.validator.addMethod('email', function (value, element) {
    var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
    return this.optional(element) || filter.test(value);
}, Drupal.t('Enter a valid email address'));

/*Phone validation*/
jQuery.validator.addMethod('phone', function (phone_number, element) {
    //phone_number = phone_number.replace(/\s+/g, "");
    return  (phone_number.length >= 10 || phone_number.length == 0) && !isNaN(phone_number);
    /*this.optional(element) || phone_number.length == 10 &&
        phone_number.match(/^\+|0(?:[0-9] ?){6,14}[0-9]$/);*/
}, Drupal.t('Enter a valid phone number'));

/*Empty Select*/
jQuery.validator.addMethod("selected", function(element, arg){
    return jQuery(arg).val() != '_none';
}, Drupal.t("filed required"));

/*Empty Ckeditor*/
jQuery.validator.addMethod("body_filled", function(element, arg){
    return jQuery(arg).contents().find("body").text() != '';
}, Drupal.t("filed required"));


/*Recaptcha*/
jQuery.validator.addMethod("recaptcha_validator", function(element, arg){
    console.log(jQuery(arg).val());
    return jQuery(arg).val() != '';
}, Drupal.t("filed required"));

